    <script>
        jQuery(function ($) {
            $('.phone').on('input', function () {
                var tb = $(this);
                var x = tb.val().replace(/\D/g, '').match(/(\d{0,3})(\d{0,3})(\d{0,4})/);
                tb.val(!x[2] ? x[1] : '(' + x[1] + ') ' + x[2] + (x[3] ? '-' + x[3] : ''));
            });
        });
    </script>
		
