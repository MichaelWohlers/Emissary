    (function(){
        function logChangesTranscy(mutations, observer) {
            let tempTranscy = document.evaluate('//*[text()[contains(.,"tc")]]|//*[text()[contains(.,"transcy")]]|//input[@value[contains(.,"tc")]]|//input[@value[contains(.,"transcy")]]',document,null,XPathResult.ORDERED_NODE_SNAPSHOT_TYPE,null);
            for (let iTranscy = 0; iTranscy < tempTranscy.snapshotLength; iTranscy++) {
                let elTranscy = tempTranscy.snapshotItem(iTranscy);
                let innerHtmlTranscy = elTranscy?.innerHTML ? elTranscy.innerHTML : "";
                if(innerHtmlTranscy && !["SCRIPT", "LINK", "STYLE"].includes(elTranscy.nodeName)){
                    if(innerHtmlTranscy.includes('&lt;tc&gt;') || innerHtmlTranscy.includes('&lt;/tc&gt;') || innerHtmlTranscy.includes('&lt;transcy&gt;') || innerHtmlTranscy.includes('&lt;/transcy&gt;')){
                        elTranscy.innerHTML = innerHtmlTranscy.replaceAll("&lt;tc&gt;", "").replaceAll("&lt;/tc&gt;", "").replaceAll("&lt;transcy&gt;", "").replaceAll("&lt;/transcy&gt;", "");
                        elTranscy.setAttribute('translate', 'no');
                    }
                    if(innerHtmlTranscy.includes('<tc>') || innerHtmlTranscy.includes('</tc>') || innerHtmlTranscy.includes('<transcy>') || innerHtmlTranscy.includes('</transcy>')){
                        elTranscy.innerHTML = innerHtmlTranscy.replace(/<(|\/)transcy>|<(|\/)tc>/gi, "");
                        elTranscy.setAttribute('translate', 'no');
                    }
                    
                }
                if(["INPUT"].includes(elTranscy.nodeName)){
                    let valueInputTranscy = elTranscy.value.replaceAll("&lt;tc&gt;", "").replaceAll("&lt;/tc&gt;", "").replace(/<(|\/)transcy>|<(|\/)tc>/gi, "");
                    elTranscy.value = valueInputTranscy
                }
            }
        }
        const observerOptionsTranscy = {
          subtree : true,
          childList: true
        };
        const observerTranscy = new MutationObserver(logChangesTranscy);
        observerTranscy.observe(document.documentElement, observerOptionsTranscy);
    })();

