              .bottom-footer span, .footer-flex-contain .center-footer-section, .footer-social-icons ul.social i, .footer-title h4, .desktop-social-title h4 { color: rgba(40,140,100,1); }
              .bottom-footer, .footer-title h4 { font-size: 12px; }
