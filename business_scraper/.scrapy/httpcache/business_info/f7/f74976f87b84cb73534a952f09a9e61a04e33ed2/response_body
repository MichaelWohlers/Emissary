                  </div>
                  <div id="logo">
                        <span class="speedometer"></span>
                        <span class="needle" :style="Number(total_fundraising) &gt; Number(fundraising_goal) ? 'transform: rotate(180deg) translate(-50%, -50%)' : 'transform: rotate(' + (((((total_fundraising*100)/fundraising_goal)) / 100) * 180) + 'deg) translate(-50%, -50%)'"></span>
                  </div>
                  <div class="gauge-mask" id="gauge_y">
                        <div class="gauge-semi-circle">
                        </div>
                        <div class="gauge-semi-circle--mask" :style="Number(total_fundraising) &gt; Number(fundraising_goal) ? 'transform: rotate(180deg)' : 'transform: rotate(' + (((((total_fundraising*100)/fundraising_goal)) / 100) * 180) + 'deg)'">
                        </div>
                        <div class="gauge-data">
                        </div>
                  </div>
                  <div class="metric-contain">
                        <div class="metrics">
                              <span class="m-title">
                                    <span>Goal:</span>
                              </span>
                              <span class="value">
                                    ${{Number(fundraising_goal).toFixed(2)}}
                              </span>
                        </div>
                        <div class="metrics">
                              <span class="m-title">
                                    <span>Raised:</span>
                              </span>
                              <span class="value">
                                    ${{Number(total_fundraising).toFixed(2)}}
                              </span>
                        </div>
                  </div>
            </section>
            <!--Gauge end-->
      </div>
</section>
