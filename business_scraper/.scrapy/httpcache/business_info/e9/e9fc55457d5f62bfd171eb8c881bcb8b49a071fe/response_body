      window.getDeviceType = function() {
        const isMobile = (typeof window.orientation !== "undefined") || (navigator.userAgent.indexOf('IEMobile') !== -1);

        if (isMobile) {
          if (window.outerWidth > 767) {
            return 'tablet';
          }

          return "mobile";
        }

        return "desktop";
      };

      
