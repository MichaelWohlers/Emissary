    float: right;
    max-width: 67%;
  }

  @media only screen and (max-width: 1110px) {
    .flow-country-picker-container {
      display: none!important;
    }
  }

  .flow-ship-to-text {
    text-transform: uppercase;
    margin-right: 10px;
  }
  .flow-country-span {
    display: flex;
    align-items: center;
  }
</style>
