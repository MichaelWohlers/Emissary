<script>

	cssSlidy({
		slidyContainerSelector: 'hero_wrapper',
		timeOnSlide: 6,
    captionSource: 'none'
	});
  $(document).ready(function() { 
    // $('.cat_button').matchHeight(false);
    $('#cat_header').click(function(e) {
      e.preventDefault();
      var fixedOffset = $('#hero_wrapper').css('margin-top');
      fixedOffset = fixedOffset.substring(0, fixedOffset.length - 2);
      $('html,body').animate({scrollTop: $('.cat_grid_wrapper').offset().top - parseInt(fixedOffset)}, '800');
      return false;
    });
  });


</script>




