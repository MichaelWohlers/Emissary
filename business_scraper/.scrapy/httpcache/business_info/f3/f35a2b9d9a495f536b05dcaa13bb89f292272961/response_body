window.cachedScript('https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js', function () {
    $('.lp-mobile-menu-click').click(function (e) {
        
        e.preventDefault();

        if ($('.lp-header-menu').hasClass('mobile-show') == true) {
          //hide its submenu

            $('.lp-header-menu').removeClass("mobile-show");
            $('.lp-header-menu').css("display", "none");

         } else {
          //show its submenu
          
          $(".lp-header-menu").fadeIn(250, function() {
            $(this).addClass("mobile-show");
            });

         }
         

});
$('.mobile-menu-close').click(function (e) {
        
        e.preventDefault();
          $('.lp-header-menu').removeClass("mobile-show");
          $('.lp-header-menu').css("display", "none");

         

});

$('.footer-menu-toggle').click(function (e) {
        
        e.preventDefault();

        if ($(this).hasClass('footer-menu-toggle-closed') == true) {
          //hide its submenu

            $(this).parent().find('.lp-footer-nav-list').css('display', 'block');
            $(this).removeClass('footer-menu-toggle-closed');
            $(this).addClass('footer-menu-toggle-open');

         } else {
          //show its submenu
          
            $(this).parent().find('.lp-footer-nav-list').css('display', 'none');
            $(this).removeClass('footer-menu-toggle-open');
            $(this).addClass('footer-menu-toggle-closed');

         }

});


$('.detail-modal-click').click(function (e) {
        
        e.preventDefault();
          $('.lp-modal-background').css("display", "block");
          $(this).parent().find('.lp-modal-wrap').css("display", "block");

         

});
$('.close-modal').click(function (e) {
        
        e.preventDefault();
          $('.lp-modal-background').css("display", "none");
          $(this).parent().css("display", "none");

         

});
$('.lp-modal-background').click(function (e) {
        
        e.preventDefault();
          $('.lp-modal-background').css("display", "none");
          $('.lp-modal-wrap').css("display", "none");

         

});

$('.open-search-click').click(function (e) {
        
        e.preventDefault();
          $('.lp-search-background').css("display", "block");
          $('.lp-search-wrap').css("display", "block");
});
$('.search-close').click(function (e) {
        
        e.preventDefault();
          $('.lp-search-background').css("display", "none");
          $('.lp-search-wrap').css("display", "none");
});
$('.lp-search-background').click(function (e) {
        
        e.preventDefault();
          $('.lp-search-background').css("display", "none");
          $('.lp-search-wrap').css("display", "none");
});


