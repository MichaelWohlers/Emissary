<script type="text/javascript">
  $(document).ready(function(){
  
//     $('.search-customcss').click(function(){
//      $('#fallbacktext').hide();
//     });
    
    $('#stickyHeaderSearchButton').click(function () {
      if ($('#stickyHeaderSearchIcon'). hasClass('fa-times')) {
        $('.sticky-header-menu > form').css("width", "0");
        $('#stickyHeaderSearchIcon').toggleClass('fa-times');
      } else {
        $('.sticky-header-menu > form').css("width", "40%");
        $('#stickyHeaderSearchIcon').toggleClass('fa-times');
      }
    });
  }); 
  
  $(window).scroll(function() {
    if ($(window).width() > 991){
      if ($(this).scrollTop() > $('.site-header__mobile-nav').height()) {
        $('#stickyHeader').fadeIn(300);
      } else {
        $('#stickyHeader').fadeOut(300);
      }
    }
  });
</script>

