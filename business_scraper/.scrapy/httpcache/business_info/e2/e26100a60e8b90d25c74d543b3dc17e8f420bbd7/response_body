    return _fn_onReady;

  })(),
  init				: function() {
    var host = document.location.hostname, path = document.location.pathname;

    if (path.indexOf('/blog') > -1) {
      window.btPageData = null;
    }
    if (!window.btPageData) {
      window.btPageData = {
        page		: {},
        site		: {},
        visitor	    : {}
      };

