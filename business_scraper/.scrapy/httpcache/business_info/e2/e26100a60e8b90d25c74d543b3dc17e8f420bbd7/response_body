  ready: (function _ready() {
    var success, error, _promise, _ready = false;

    _promise = new Promise(function __promise( res, rej ) {
      success = res;
      error = rej;
    });

    var _fn_onReady = function() {
      if ( _ready ) return Promise.resolve();

      return _promise;
    }

    _fn_onReady._init = function() {
      _ready = true;
      success();
    }

