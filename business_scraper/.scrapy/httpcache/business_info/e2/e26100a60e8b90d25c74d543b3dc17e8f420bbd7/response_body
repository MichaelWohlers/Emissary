      if (host == 'stores.express.com' || host.indexOf('expressfactoryoutlet.com') > -1 || path.indexOf('/blog') > -1) {
        btPageData.page = {
          categoryName: host.indexOf('stores') > -1 ? 'Stores' : 'non-browse',
          name: '',
          serverTime: '',
          type: ''
        };
        btPageData.site = {
          isProduction: true,
          type: 'responsive'
        };

        if (path.indexOf('/blog') > -1) {
          btPageData.page.type = 'Blog';
        } else if (host == 'www.expressfactoryoutlet.com') {
          btPageData.page.type = 'EFO';
        } else if (host == 'stores.expressfactoryoutlet.com') {
          btPageData.page.type = 'EFO Stores';
        } else {
          btPageData.page.type = 'Stores';
        }

        var path = document.location.pathname.replace('/blog', ''), pathEls;
        if (path.charAt(0) == '/') {
          path = path.substring(1);
        }
        if (path == '') {
          path = 'Home';
        } else if (path == 'search') {
          path += '/results';
        }
        pathEls = path.split('/');
        for (var i=0; i<pathEls.length; i++) {
          if (i < 2 && pathEls[i].length == 2) {
            pathEls[i] = pathEls[i].toUpperCase();
          } else {
            pathEls[i] = pathEls[i].charAt(0).toUpperCase() + pathEls[i].substring(1).toLowerCase();
          }

          pathEls[i] = pathEls[i].replace(/\-/g, ' ');
          var el = '';
          for (var j=0; j<pathEls[i].length; j++) {
            if (pathEls[i].charAt(j - 1) == ' ') {
              el += pathEls[i].charAt(j).toUpperCase();
            } else {
              el += pathEls[i].charAt(j);
            }
          }
          pathEls[i] = el;
        }
        btPageData.page.name = btPageData.page.type + ' : ' + pathEls.join(' : ');
      } else if (host.match(/editor\.(qa\.)?express\.com/) !== null) {
        btPageData.site.isProduction = host.indexOf('qa') > -1 ? false : true;
        _dataManager.bindAMITracking();
        _dataManager.setAMIDataLayer();
      }
    } else if (host == 'www.expressfactoryoutlet.com') {
      btPageData.page.type = 'EFO';
      btPageData.site.isProduction = true;
      var path = document.location.pathname.replace('/blog', ''), pathEls;
      if (path.charAt(0) == '/') {
        path = path.substring(1);
      }
      if (path == '') {
        path = 'Home';
      } else if (path == 'search') {
        path += '/results';
      }
      pathEls = path.split('/');
      for (var i=0; i<pathEls.length; i++) {
        if (i < 2 && pathEls[i].length == 2) {
          pathEls[i] = pathEls[i].toUpperCase();
        } else {
          pathEls[i] = pathEls[i].charAt(0).toUpperCase() + pathEls[i].substring(1).toLowerCase();
        }

        pathEls[i] = pathEls[i].replace(/\-/g, ' ');
        var el = '';
        for (var j=0; j<pathEls[i].length; j++) {
          if (pathEls[i].charAt(j - 1) == ' ') {
            el += pathEls[i].charAt(j).toUpperCase();
          } else {
            el += pathEls[i].charAt(j);
          }
        }
        pathEls[i] = el;
      }
      btPageData.page.name = btPageData.page.type + ' : ' + pathEls.join(' : ');
    }
    
    // redirect search terms
    if (_satellite.getVar('Query_redirect') && _satellite.getVar('Query_redirect') === 'true') {
	  btPageData.productList = btPageData.productList || {};
	  btPageData.productList.search = btPageData.productList.search || {};
	  btPageData.productList.search.keyword = _dataManager.getParam('q', document.referrer) || '';
    }
    
    
    if (!window.isReact && window.jQuery) {
      jQuery(function() {
        _dataManager.bindJQueryEventHandlers(jQuery);
      });
    }

    _dataManager.enhanceDataLayer();
    _dataManager.initialized = true;
    _dataManager.ready._init();

    // bind event handlers
    //_dataManager.bindEventHandlers();
    if (document.location.href.indexOf('/checkout/') == -1) {
      _satellite.track('pageView');

      if (btPageData.page.type == 'Product') {
        _satellite.track('pdpView');
        _satellite.track('productView');
      } else if (btPageData.page.type == 'Category') {
        _satellite.track('categoryView');
      } else if (btPageData.page.type == 'Search') {
        _satellite.track('searchView');
      } else if (btPageData.page.type == 'Shopping Cart' && btPageData.page.name == 'Shopping Cart : View Cart') {
        _satellite.track('trackBag');
      }

    }
  },

  setAMIDataLayer	: function() {
    var type = _satellite.getVar('AMI_Page_Type');
    switch(type) {
      case 'buyer#Buyer::SellerShopController#index':
        btPageData.page.type = btPageData.page.section = btPageData.page.subSection = 'Editor Profile';
        btPageData.page.name = btPageData.page.type + ' : ' + _satellite.getVar('AMI_Editor_Name');
        break;
      case 'buyer#Buyer::ProductCollectionsController#show':
        btPageData.page.type = btPageData.page.section = btPageData.page.subSection = 'Editor Collection';
        btPageData.page.name = 'Collection : ' + _satellite.getVar('AMI_Editor_Name') + ' : ' + _satellite.getVar('AMI_Collection_Name');
        break;
      case 'buyer#Buyer::ProductsController#show':
        btPageData.page.type = btPageData.page.section = btPageData.page.subSection = 'Product';
        btPageData.page.name = 'Product : ' + _satellite.getVar('AMI_Product_Name');
        btPageData.items = [
          {
            id		: _satellite.getVar('AMI_Product_ID'),
            name	: _satellite.getVar('AMI_Product_Name')
          }
        ];
        break;
      case 'buyer#Buyer::User::Contact::LeadsController#new':
        btPageData.page.type = btPageData.page.section = btPageData.page.subSection = 'Contact Form';
        btPageData.page.name = 'Contact Editor : ' + _satellite.getVar('AMI_Editor_Name');
        break;
      case 'buyer#Buyer::SellerShopController#thanks':
        btPageData.page.type = btPageData.page.section = btPageData.page.subSection = 'Contact Form';
        btPageData.page.name = 'Contact Editor : ' + _satellite.getVar('AMI_Editor_Name') + ' : Confirmation';
        break;
    }
  },

  bindAMITracking	: function() {
    if (window.$) {
      $(document).on('toggle-active.click', function(e, data) {
        btPageData.page.name = btPageData.page.type + ' : ' + _satellite.getVar('AMI_Editor_Name') + ' : ' + data.text;
        btPageData.eventData = {
          eventName	: 'AMI Toggle Click'
        };
        _satellite.track('pageView');
      });

      $(document).on('turbolinks:load', function(e, data) {
        _dataManager.setAMIDataLayer();
        _satellite.track('pageView');

        if (btPageData.page.type === 'Product') {
          _satellite.track('pdpView');
        }
      });

      $(document).on('product.added-to-cart', function(e, data) {
        btPageData.eventData = {
          eventName	: 'Cart Add',
          items		: [{
            id			: data.product.external_id,
            sku			: data.variant.sku,
            quantity	: data.quantity,
            name		: btPageData.items[0].name
          }]
        };

        _satellite.track('pageEvent');

        // Trigger for tracking tags on cart add
        _satellite.track('cartAdd');
      });
    }
  },

  enhanceDataLayer	: function() {
    if (!(window.btPageData && btPageData.page && btPageData.page.name && btPageData.page.type)) {
      var path = document.location.pathname;
      btPageData.page.name = path.replace(/\//gi, ' : ');
      btPageData.page.type = 'Other';
    }

    var pageEls = btPageData.page.name.split(' : '), type = btPageData.page.type, section = subSection = subSubSection = '';
    if (type == 'Product') {
      section = 'Products';
      subSection = section + ' : ' + (pageEls[0] == 'Ensemble Detail' ? 'Ensemble' : 'Solo');
      subSubSection = subSection + (pageEls[1] ? ' : ' + pageEls[1] : '');
    } else {
      section = subSection = subSubSection = pageEls[0];
      if (type == 'Other' && window.location.pathname.match(/\/(my\-account|login)/g) !== null) {
        btPageData.page.type = 'Account';
        section = 'My Account';
        btPageData.page.name = section;
        var pathEls = window.location.pathname.replace(/\-/g, ' ').split('/');
        for (var i=0; i<pathEls.length; i++) {
          var el = '';
          if (pathEls[i] == 'login') {
            pathEls[i] = "Sign In Or Register"
          }
          for (var j=0; j<pathEls[i].length; j++) {
            if (j == 0 || pathEls[i].charAt(j-1) == ' ') {
              el += pathEls[i].charAt(j).toUpperCase();
            } else {
              el += pathEls[i].charAt(j);
            }
          }
          if (el && el !== section) {
            btPageData.page.name += (btPageData.page.name ? ' : ' : '') + el;
          }
        }

        if (btPageData.page.name == 'My Account') {
          btPageData.page.name += ' : Express Next Dashboard';
        }

        pageEls = btPageData.page.name.split(' : ');
      }
      if (pageEls.length > 1) {
        subSection = subSubSection = (section + ' : ' + pageEls[1]);
        if (pageEls.length > 2) {
          subSubSection += ' : ' + pageEls[2];
        }
      }
    }
    btPageData.page.section = section;
    btPageData.page.subSection = subSection;
    btPageData.page.subSubSection = subSubSection;

    if (!btPageData.site) {
      btPageData.site = {};
    }
    if (!btPageData.site.type) {
      btPageData.site.type = 'responsive';
    }

    btPageData.site.isProduction = document.location.hostname.match(/(www|stores)\.express(factoryoutlet)?\.com/) !== null ? true : false;
  },

  bindStyliticsEventHandlers	: function() {
    var styliticsCheck = setInterval(function() {
      if (window.injectedStyliticsFeaturedShop) {
        clearInterval(styliticsCheck);
        var myShop = window.injectedStyliticsFeaturedShop;
        var evts = [
          'shop-click',
          'theme-click',
          'outfit-click',
          'item-click'
        ];
        for (var i=0; i<evts.length; i++) {
          myShop.on(evts[i], function(e) {
            if (e.subject && e.action && e.action === 'click') {
              var action = '';
              switch(e.subject) {
                case 'shop':
                case 'theme':
                case 'outfit':
                  if (e.subject === 'outfit') {
                    action = 'outfit click:' + e.outfit_id;
                  }
                  btPageData.eventData = {
                    eventName: 'Stylitics Featured Shop Interaction',
                    featuredShopInteraction: action
                  }
                  _satellite.track('pageEvent');
                  break;
                case 'item':
                  btPageData.eventData = {
                    eventName			: 'View Stylitics Outfit in Express View',
                    recommendationType	: 'Stylitics Featured Shop',
                    items				: [{
                      id: e.item_id,
                      name: e.item_name || '',
                      outfitId: e.outfit_id
                    }]
                  };

                  _satellite.track('pageEvent');
                  _satellite.track('expressView');
                  _satellite.track('productView');
                  break;
              }
            }
          });
        }
      }
    }, 500);
  },

  bindReactEventHandlers		: function() {
    // page load event
    window.addEventListener('REACTReady', function(e) {
      btPageData.eventData = {};
      var pageData = e.detail, pagePath = window.location.pathname;

      if (btPageData.page && btPageData.page.type) {
        btPageData.page.previousType = btPageData.page.type;
      }
      
      btPageData.visitor = _dataManager.getCustomerData(pageData);
      
      if (pagePath.indexOf('/cat') > -1) {
        var catUrl = pageData.history.location.pathname;
        var catGender = '';
        if (catUrl.indexOf('/mens-clothing/') > -1) {
          catGender = 'Mens';
        } else if (pagePath.indexOf('/womens-clothing/') > -1) {
          catGender = 'Womens';
        }
        var catId = catUrl.substring(catUrl.indexOf('/cat') + 1);
        catId = catId.indexOf('/') > -1 ? catId.substring(0, catId.indexOf('/')) : catId;
        if (catId) {
          btPageData.page.categoryId = catId;
        }
        btPageData.items = [];
        btPageData.page.type = 'Category';
        btPageData.page.section = catGender;
        btPageData.page.subSection = btPageData.page.section + ' : Landing';
        btPageData.page.subSubSection = btPageData.page.subSection + ' : ' + pageData.categoryName;
        btPageData.page.name = btPageData.page.subSubSection;
        btPageData.page.path = catUrl;
        if ((e.detail && e.detail.inlineContentBlocks && e.detail.inlineContentBlocks.desktop && e.detail.inlineContentBlocks.desktop.length > 0) || (e.detail && e.detail.inlineContentBlocks && e.detail.inlineContentBlocks.mobile && e.detail.inlineContentBlocks.mobile.length > 0)) {
          btPageData.page.inlineContentFlag = 'True';
        } else {
          btPageData.page.inlineContentFlag = 'False';
        }
        btPageData.productList = _dataManager.getProductList(pageData);
      } else if (pagePath.indexOf('/search') > -1) {
        btPageData.page.type = btPageData.page.section = 'Search';
        btPageData.page.name = btPageData.page.subSection = btPageData.page.subSubSection = btPageData.page.type + ' : Results';
        btPageData.productList = _dataManager.getProductList(pageData);
        btPageData.items = [
          {
            searchTerm : pageData.searchTerm || '',
          }
        ];
      } else if (pagePath.indexOf('/pro/') > -1) {
        btPageData.productList = {};
        var productData = pageData.product || pageData.data.product, gender = '';
        btPageData.page.type = 'Product';
        if (pagePath.indexOf('/clothing/men/') > -1) {
          gender = 'Mens';
        } else if (pagePath.indexOf('/clothing/women/') > -1) {
          gender = 'Womens';
        }
        btPageData.page.name = (gender ? gender + ' : ' : '') + 'Product Detail : ' + productData.name;
        var pageEls = btPageData.page.name.split(' : '), section = subSection = subSubSection = '';
        section = 'Products';
        subSection = section + ' : ' + (pageEls[0] == 'Ensemble Detail' ? 'Ensemble' : 'Solo');
        subSubSection = subSection + (pageEls[1] ? ' : ' + pageEls[1] : '');
        btPageData.page.section = section;
        btPageData.page.subSection = subSection;
        btPageData.page.subSubSection = subSubSection;
        if (productData.__typename == 'Product' || productData.__typename == 'EnsembleProduct') {
          var prodArr =  _dataManager.getProductData(pageData.products ? pageData.products : [pageData], pageData);
          if (prodArr) {
            btPageData.items = prodArr;
          }
        }
      } else if (pagePath == '/bag') {
        btPageData.page.type = 'Shopping Cart';
        btPageData.page.name = 'Shopping Cart : View Cart';
        if (btPageData.eventData) {
          btPageData.eventData = {};
        }

        var productData;
        var savedProducts;
        if (pageData.getOrderSummary && pageData.getOrderSummary.orderSummary && pageData.getOrderSummary.orderSummary.lineItems) {
          productData = pageData.getOrderSummary.orderSummary.lineItems;
        } else if (pageData.orderSummary) {
          orderData = pageData.orderSummary.lineItems;
        }
        if (productData && productData.length > 0) {
          btPageData.items = _dataManager.getProductData(productData, pageData);
        }
        if (pageData.getShoppingList && pageData.getShoppingList.getShoppingList && pageData.getShoppingList.getShoppingList.lineItems) {
          savedProducts = pageData.getShoppingList.getShoppingList.lineItems;
        }
        if (savedProducts && savedProducts.length > 0) {
          btPageData.savedItems = _dataManager.getProductData(savedProducts, pageData);
        } else {
          btPageData.savedItems = [];
        }
      } else if (pagePath.indexOf('/my-account/') > -1) {
        btPageData.page.type = 'Account';
        section = 'My Account';
        btPageData.page.name = section;
        var pathEls = window.location.pathname.replace(/\-/g, ' ').split('/');
        for (var i=0; i<pathEls.length; i++) {
          var el = '';
          if (pathEls[i] == 'login') {
            pathEls[i] = "Sign In Or Register"
          }
          for (var j=0; j<pathEls[i].length; j++) {
            if (j == 0 || pathEls[i].charAt(j-1) == ' ') {
              el += pathEls[i].charAt(j).toUpperCase();
            } else {
              el += pathEls[i].charAt(j);
            }
          }
          if (el && el != section) {
            btPageData.page.name += (btPageData.page.name ? ' : ' : '') + el;
          }
        }

        if (btPageData.page.name == 'My Account') {
          btPageData.page.name += ' : Express Next Dashboard';
        }

        btPageData.page.subSection = btPageData.page.subSubSection = btPageData.page.name;

      } else if (pagePath.indexOf('featured-shops') > -1) {
        btPageData.page.name = '';
        btPageData.page.type = btPageData.page.section = btPageData.page.subSection = btPageData.page.subSubSection = 'Featured Shops';

        var pathEls = pagePath.substring(1).replace(/\-/g, ' ').split('/');
        for (var i=0; i<pathEls.length; i++) {
          var el = '';
          for (var j=0; j<pathEls[i].length; j++) {
            if (j == 0 || pathEls[i].charAt(j-1) == ' ') {
              el += pathEls[i].charAt(j).toUpperCase();
            } else {
              el += pathEls[i].charAt(j);
            }
          }
          if (el) {
            if (i == 2) {
              btPageData.page.subSection += ' : ' + el;
              btPageData.page.subSubSection += ' : ' + el;
            }
            if (i == 3) {
              btPageData.page.subSubSection += ' : ' + el;
            }
            btPageData.page.name += (btPageData.page.name ? ' : ' : '') + el;
          }
        }
        _dataManager.bindStyliticsEventHandlers();
      } else {
        btPageData.page.type = 'Other';
        btPageData.page.name = '';
        var pathEls = pagePath.split('/');
        for (var i=0; i<pathEls.length; i++) {
          if (pathEls[i] != '') {
            btPageData.page.name += (btPageData.page.name ? ' : ' : '') + pathEls[i].charAt(0).toUpperCase() + pathEls[i].substring(1);
          }
        }
      }

      btPageData.page.lastType = btPageData.page.type;
      if (_dataManager.initialized && pagePath.indexOf('/checkout/') == -1) {
        _satellite.track('pageView');
        if (btPageData.page.type == 'Product') {
          _satellite.track('pdpView');
          _satellite.track('productView');
        } else if (btPageData.page.type == 'Category') {
          _satellite.track('categoryView');
        } else if (btPageData.page.type == 'Search') {
          _satellite.track('searchView');
        } else if (btPageData.page.type == 'Shopping Cart' && btPageData.page.name == 'Shopping Cart : View Cart') {
          _satellite.track('trackBag');
        }
      } else {
        _dataManager.init();
      }
    });

    // User login event
    window.addEventListener('AccountReady', function() {
      btPageData.eventData = {
        eventName:  'Customer loggedIn'
      };
      _satellite.track('pageEvent');
      _satellite.track('loginEvent');
    });

    // PLP update product list
    window.addEventListener('updateProductList', function(e) {
      btPageData.productList = _dataManager.getProductList(e.detail);

      btPageData.eventData = {
        eventName: 'Update Product List'
      };
      _satellite.track('pageView');
    });

    // PDP cart add
    window.addEventListener('cartItemAdd', function(e) {
      btPageData.eventData = {
        eventName	: 'Cart Add',
        recommendationType	: e.detail.recsAlgorithm || '',
        items		: _dataManager.getProductData([e.detail.data || e.detail], e.detail)
      };

      _satellite.track('pageEvent');

      // Trigger for tracking tags on cart add
      _satellite.track('cartAdd');
    });

    // Checkout BOPIS toggle
    window.addEventListener('SthToBopis', function(e) {
      btPageData.eventData = {
        eventName	: 'Select BOPIS for Item',
        items		: _dataManager.getProductData([e.detail.data || e.detail], e.detail)
      };

      _satellite.track('pageEvent');
    });

    // Checkout Ship to Home toggle
    window.addEventListener('bopisToSth', function(e) {
      btPageData.eventData = {
        eventName	: 'Select STH for Item',
        items		: _dataManager.getProductData([e.detail.data || e.detail], e.detail)
      };

      _satellite.track('pageEvent');
    });

    // Checkout Select BOPIS Store
    window.addEventListener('pickUpHere', function(e) {
      btPageData.eventData = {
        eventName	: 'Select BOPIS Store',
        items		: _dataManager.getProductData([e.detail.data || e.detail], e.detail)
      };

      _satellite.track('pageEvent');
    });

    // PDP Express View
    window.addEventListener('expressView', function(e) {
      btPageData.eventData = {
        eventName			: 'Express View',
        recommendationType	: e.detail.recsAlgorithm || '',
        items				: _dataManager.getProductData([e.detail], e.detail)
      };

      _satellite.track('pageEvent');
      _satellite.track('expressView');
      _satellite.track('productView');
    });

    // PDP Page load with stylitics outfits on page
    window.addEventListener('pdpStyliticsReady', function(e) {
      btPageData.page.styliticsFlag = 'True';
      btPageData.eventData = {
        eventName			: 'PDP Stylitics Page',
        outfits				: e.detail.outfits || ''
      };

      _satellite.track('pageEvent');
    });

    // View a product in stylitics express view
    window.addEventListener('pdpStyliticsQuickViewReady', function(e) {
      btPageData.eventData = {
        eventName			: 'Tap on Stylitics Outfit',
        outfitId			: e.detail.outfitData.id
      };
    });

    // Open Stylitics outfit in Express View
    window.addEventListener('pdpSelectStyliticsItem', function(e) {
      var prod = e.detail.itemData, outfitId = btPageData.eventData && btPageData.eventData.outfitId ? btPageData.eventData.outfitId : '';
      if (prod && outfitId) {
        btPageData.eventData = {
          eventName			: 'View Stylitics Outfit in Express View',
          items:				[{
            colorCode				: '',
            id						: prod.productId,
            name					: prod.name,
            outfitId				: outfitId,
            recommendationType		: 'stylitics',
          }],
          outfitId					: btPageData.eventData.outfitId
        };
      }

      _satellite.track('pageEvent');
    });

    // Add stylitics product to bag
    window.addEventListener('cartItemAddStylitics', function(e) {
      var prod, outfitId = btPageData.eventData && btPageData.eventData.outfitId ? btPageData.eventData.outfitId : '';
      if (e.detail.withInventory && e.detail.withInventory.product && e.detail.withInventory.product[e.detail.productId]) {
        prod = e.detail.withInventory.product[e.detail.productId].productData;
      }
      if (prod && outfitId) {
        btPageData.eventData = {
          eventName			: 'Cart Add',
          items:				[{
            colorCode				: '',
            id						: prod.productId,
            name					: prod.name,
            outfitId				: outfitId,
            gender					: prod.gender,
            inStock					: true,
            isBopis					: false,
            isLowInventory			: false,
            isMarketplaceProduct	: prod.marketPlaceProduct || false,
            name					: prod.name,
            originalPrice			: prod.listPrice ? parseFloat(prod.listPrice.substring(1)) : '',
            price					: prod.salePrice ? parseFloat(prod.salePrice.substring(1)) : '',
            promoMessage			: prod.promoMessage || '',
            quantity				: e.detail.quantity,
            recommendationType		: 'stylitics',
            sellerId				: '',
            sku						: e.detail.skuId,
            url						: prod.productURL || ''
          }],
          outfitId					: btPageData.eventData.outfitId
        };
      }

      _satellite.track('pageEvent');
    });

    // PDP find in store event
    window.addEventListener('findInStore', function(e) {
      btPageData.eventData = {
        eventName		: 'Find in Store',
        findInStore		: {
          event		: '',
          productId	: (function() {
            if (typeof btPageData.items !== 'undefined'
              && btPageData.items.length
              && btPageData.items[0]
            ) return btPageData.items[0].id;

            return null;
          })(),
          stepName	: e.detail.stepName || '',
          storeId		: e.detail.user.customerStore.storeId || '',
          storeZipcode : e.detail.user.customerStore.postalCode
        }
      };
      _satellite.track('checkAvailability');
      _satellite.track('pageEvent');
    });

    //Credit card analyitics
    window.addEventListener('enccCompleted', function (e) {
      btPageData.productList = _dataManager.getProductList(e.detail);

      btPageData.eventData = {
        eventName: 'Credit Card Application Completed'
      };
      _satellite.track('enccComplete');
    });

    window.addEventListener('enccStarted', function (e) {
      btPageData.productList = _dataManager.getProductList(e.detail);

      btPageData.eventData = {
        eventName: 'Credit Card Application Started'
      };
      _satellite.track('enccStart');
    });

    // enter promo code
    window.addEventListener('enteredPromoCode', function(e) {
      var order = (e.detail.getOrderSummary && e.detail.getOrderSummary.orderSummary) || e.detail.orderSummary, code = '';
      if (order && order.promotions && order.promotions[0] && order.promotions[0].code) {
        code = order.promotions[0].code;

        btPageData.eventData = {
          eventName		: 'Entered Promo Code',
          promoCode		: code
        };
        _satellite.track('pageEvent');
      }
    });

    // cart remove
    window.addEventListener('scRemove', function(e) {
      btPageData.eventData = {
        eventName		: 'Cart Remove',
        items			: _dataManager.getProductData([e.detail], e.detail),
        action			: e.detail.action
      };

      _satellite.track('pageEvent');
      _satellite.track('cartRemove');
    });

    // move to bag from SFL
    window.addEventListener('moveToBagBtn', function(e) {
      btPageData.eventData = {
        eventName		: 'Cart Add',
        items			: _dataManager.getProductData([e.detail], e.detail),
        action			: 'sfl'
      };

      _satellite.track('pageEvent');

      // Trigger for tracking tags on cart add
      _satellite.track('cartAdd');
    });

    // shop by model/size
    window.addEventListener('shopByModelButtonClick', function(e) {
      btPageData.eventData = {
        eventName		: 'Shop by Model Button Click',
        items			: btPageData.items || ''
      };

      _satellite.track('pageEvent');
    });

    window.addEventListener('shopByModelSizeSelection', function(e) {
      btPageData.eventData = {
        eventName		: 'Shop by Model Size Selection',
        items			: btPageData.items || ''
      };

      _satellite.track('pageEvent');
    });

    window.addEventListener('saveForLaterBtn', function(e) {
      btPageData.eventData = {
        eventName: 'Move To Save For Later',
      }

      _satellite.track('pageEvent');
    });
    
    // checkout
    window.addEventListener('scCheckout', function(e) {
      function _getPriceAmount(_details, _price) {
        if (!_details[_price]) return 0;
        return _details[_price].amount || 0;
      }
      var pageData = e.detail, checkoutPage = window.location.pathname;
      btPageData.page.type = 'Checkout';
      btPageData.page.name = 'Shopping Cart : ';
      if (checkoutPage == '/checkout/order-summary' || checkoutPage.indexOf('/checkout/order-confirmation') === 0) {
        /*This is incase Jon asks more questions about the social seller logic for CJ.
          The "pageData.getOrderSummary.socialSellerCart" comes from app_express. This value is a boolean that denotes if a customer on Express.com is a
          style editor/social seller. Since "btPageData.visitor.loyaltyStatus" is only defined on a user who is logged in the value will default to
          "Activated". The check is to override that value and call the styleEditor an "Editor".
          The '' undefined check is for guests, so if a guest checksout they will not be flagged as a social seller/style editor.
        */
        if(pageData.getOrderSummary.socialSellerCart) {
          if(btPageData.visitor.loyaltyStatus !== '' || btPageData.visitor.loyaltyStatus !== undefined) {
            btPageData.visitor.loyaltyStatus = 'Editor';
          }
        }
        btPageData.page.name += 'Confirmation';
      } else {
        btPageData.page.name += 'Single Screen : ';
        if (checkoutPage == '/checkout/contact-information') {
          btPageData.page.name += 'Contact';
        } else if (checkoutPage == '/checkout/delivery-and-pickup') {
          btPageData.page.name += 'Delivery and Pickup';
        } else if (checkoutPage == '/checkout/pick-up-person') {
          btPageData.page.name += 'Pick Up Person';
        } else if (checkoutPage == '/checkout/shipping-and-billing-address') {
          btPageData.page.name += 'Billing';
        } else if (checkoutPage == '/checkout/payment') {
          btPageData.page.name += 'Payment';
        }
      }
      btPageData.page.subSection = btPageData.page.subSubSection = btPageData.page.name.replace(/\>/g, ':');
      
      btPageData.visitor = _dataManager.getCustomerData(pageData);
      
      var orderData;
      if (pageData.getOrderSummary) {
        orderData = pageData.getOrderSummary.orderSummary ? pageData.getOrderSummary.orderSummary : pageData.getOrderSummary;
      } else if (pageData.orderSummary) {
        orderData = pageData.orderSummary.orderSummary;
      }

      if (orderData) {
        if (orderData.contactInfo && orderData.contactInfo.email && !btPageData.visitor.emailAddress) {
          btPageData.visitor.emailAddress = orderData.contactInfo.email;
        }
        btPageData.items = _dataManager.getProductData(orderData.lineItems, pageData);

        if (orderData.status && ['processing', 'submitted', 'confirmed'].indexOf(orderData.status.toLowerCase()) !== -1) {
          // track order
          var order = {};
          order.city = orderData.billingAddress.city;
          order.countryCode = orderData.billingAddress.country;
          order.zipCode = orderData.billingAddress.postalCode;
          order.discount = Math.abs(_getPriceAmount(orderData.priceDetails, 'orderPromotionTotal'));
          order.discount += Math.abs(_getPriceAmount(orderData.priceDetails, 'tenderTypePromo'));
          order.roundup = _getPriceAmount(orderData.priceDetails, 'roundUpForCharityAmount');
          order.giftBoxes = Math.round(_getPriceAmount(orderData.priceDetails, 'giftWrapAmount') / 5);
          order.giftCard = _getPriceAmount(orderData.priceDetails, 'giftCardTotal');
          order.id = (orderData.orderId.indexOf('-') === -1) ? orderData.orderId : orderData.orderNumber;
          order.merchandise = _getPriceAmount(orderData.priceDetails, 'subTotalAmount');
          order.deliveryType = orderData.deliveryType;
          order.totalItems = 0;
          order.sellerName = orderData.sellerName || '';
          order.sellerId = orderData.sellerId || '';
          order.socialSeller = orderData.socialSellerCart || false;
          for (var i=0; i<btPageData.items.length; i++) {
            order.totalItems += btPageData.items[i].quantity;
          }
          order.promoCode = '';
          for (var i=0; i<orderData.promotions.length; i++) {
            if (orderData.promotions[i].code) {
              order.promoCode += (order.promoCode ? '|' : '') + orderData.promotions[i].code;
            }
          }
          order.rewards = 0;
          order.rewardsUsed = 0;
          for (var i=0; i<orderData.rewards.length; i++) {
            order.rewards += parseInt(orderData.rewards[i].displayAmount.substr(1), 10);
            order.rewardsUsed++;
          }

          var methods = [];
          if (order.giftCard > 0) {
            methods.push('Gift Card');
          }
          if (order.rewards > 0) {
            methods.push('Rewards');
          }

          for (var i=0; i<orderData.payments.length; i++) {
            var paymentType = (orderData.payments[i].paymentType || '').toLowerCase();
            if (paymentType.match(/credit-?card|expressprivatelabelcard|klarna|vic|expresscard|paypal|apple/gi) !== null) {
              var type = '', cardType = orderData.payments[i].attributes.CREDIT_CARD_TENDER_TYPE ? orderData.payments[i].attributes.CREDIT_CARD_TENDER_TYPE.toLowerCase() : '';
              if (cardType) {
                switch (cardType) {
                  case 'expresscard':
                    type = 'ExpressCard';
                    break;
                  case 'expressprivatelabelcard':
                    type = 'ExpressCard';
                    break;
                  case 'americanExpress':
                    type = 'americanExpress';
                    break;
                  case 'mastercard':
                    type = 'masterCard';
                    break;
                  case 'paypal':
                    type = 'PayPal';
                    break;
                  case 'discover':
                    type = 'discover';
                    break;
                  case 'klarna':
                  case 'vic':
                    type = 'klarna';
                    break;
                  default:
                    type = cardType.charAt(0).toUpperCase() + cardType.substring(1);
                    break;
                }
              }
              
              if (paymentType === 'applepay') {
                type = 'applepay';
              }

              var found = false;
              for (var j=0; j<methods.length; j++) {
                if (methods[j] == type) {
                  found = true;
                  break;
                }
              }
              if (!found) {
                methods.push(type);
              }
            }
          }

          order.paymentMethod = methods.sort().join(' | ');
          if (order.paymentMethod.indexOf(' | ') == 0) {
            order.paymentMethod = order.paymentMethod.replace(' | ', '');
          }

          if(pageData.getOrderSummary.priceDetails !== undefined && pageData.getOrderSummary !== undefined){
            if(pageData.getOrderSummary.priceDetails.salesTaxes.amount){
              order.salesTax = pageData.getOrderSummary.priceDetails.salesTaxes.amount;
            }else{
              order.salesTax = 0;
            }
          }else{
            order.salesTax = 0;
          }

          order.shipping = _getPriceAmount(orderData.priceDetails, 'shippingPrice');
          order.shippingMethod = '';

          if (orderData.shippingDestinations && orderData.shippingDestinations[0]) {
            if (orderData.shippingDestinations[0].shippingAddress) {
              order.shippingZipCode = orderData.shippingDestinations[0].shippingAddress.postalCode;
            }

            if (orderData.shippingDestinations[0].shippingMethod) {
              order.shippingMethod = orderData.shippingDestinations[0].shippingMethod.name;
              if (order.shippingMethod == 'CustomerPickup') {
                order.shippingMethod = 'Store Pickup | ' + orderData.customerStoreInfo.orderStore.name + ' : ' + orderData.customerStoreInfo.orderStore.state;
              }
            }
          }
          order.state = orderData.billingAddress.state;
          order.subtotalMinusDiscount = order.merchandise - (order.discount || 0) - (order.rewards || 0);
          order.subtotalMinusDiscount = Math.round(order.subtotalMinusDiscount * 100) / 100;
          order.total = order.merchandise + order.shipping  + order.discount + order.rewards;
          if (orderData.sellerInfo && orderData.sellerInfo.sellerName) {
            order.sellerName = orderData.sellerInfo.sellerName;
          }
          btPageData.order = order;
          _satellite.track('trackOrderSummary');
        }
      }

      btPageData.eventData = {
        eventName	: 'Checkout Step Change'
      };
      _satellite.track('pageView');
    });

    // wolps events
    window.addEventListener('wolpsInvite', function(e) {
      btPageData.eventData = {
        eventName		: 'WOLPS Invite',
        virtualPageView	: true
      };

      _satellite.track('pageView');
    });

    window.addEventListener('wolpsApplication', function(e) {
      btPageData.eventData = {
        eventName		: 'WOLPS Application Start',
        virtualPageView	: true
      };

      _satellite.track('pageView');
    });

    window.addEventListener('wolpsNotification', function(e) {
      btPageData.eventData = {
        eventName			: 'WOLPS Application Submitted',
        virtualPageView		: true,
        applicationStatus	: (e.detail && e.detail.location && e.detail.location.state && e.detail.location.state.status ? e.detail.location.state.status : 'no status provided')
      };
      _satellite.track('pageView');
    });
    //Bold Metrics tracking events
    window.addEventListener('BoldMetricsForm', function(e) {
      btPageData.eventData = {
        eventName			: 'Bold Metrics Form Opened',
        items				: [{id: e.detail.productId}]
      };
      _satellite.track('pageEvent');
    });

    window.addEventListener('BoldMetricsFormComplete', function(e) {
      console.log('BoldMetricsFormComplete event triggered');
    });

    window.addEventListener('BoldMetricsResult', function(e) {
      btPageData.eventData = {
        eventName			: 'Bold Metrics Results Shown',
        items				: [{id: e.detail.productId, size: e.detail.perfectSize}]
      };
      _satellite.track('pageEvent');
    });

    // Color Swatch Click
    window.addEventListener('colorSwatchSelected', function(e) {
      var _items = btPageData && btPageData.items;

      if (!_items || (_items && !_items.length)) {
        _items = [{}];
      }

      var _swatchName = e.detail
        && e.detail.selectedColorName;

      if (_swatchName) {
        _items[0].swatchColor = _swatchName.toLowerCase();
        btPageData.eventData = {
          eventName			: 'PDP Swatch Click',
          items				: _items
        };
        _satellite.track('pageEvent');
      }
    });

    // Handle Bag Peek view on React Pages
    window.addEventListener('ViewedBagPeek', function() {
      btPageData.eventData = {
        eventName	: 'Bag Peek View'
      };
      _satellite.track('pageEvent');
      _satellite.track('ViewedBagPeek');
    });

    // Bluecore email events
    window.addEventListener('express_MarketingEvent', function(e) {
      if (e.detail && e.detail.eventName && e.detail.modalName)
        btPageData.eventData = {
          eventName			: 'Bluecore ' + (e.detail.ctaName ? 'Submit Form' : 'View Overlay'),
          bluecoreModalName	: e.detail.modalName + (e.detail.ctaName ? ':' + e.detail.ctaName : '')
        };
      _satellite.track('pageEvent');
    });

    window.addEventListener("ChangeStoreClicked", function(e){
      if (e.detail && e.detail.eventName && e.detail.modalName)
        btPageData.eventData = {
          eventName: "ChangeStoreClickedSMS",
        };
      _satellite.track('pageEvent');
    });

    window.addEventListener('ttpHeaderModalDisplayed', function() {
      btPageData.eventData = {
        eventName	: 'ttpHeaderModalDisplayed'
      };
      _satellite.track('pageEvent');
    });

    window.addEventListener('ttpMyAccountModalAccepted', function() {
      btPageData.eventData = {
        eventName	: 'ttpMyAccountModalAccepted'
      };
      _satellite.track('pageEvent');
    });

    window.addEventListener('ttpMyAccountModalDisplayed', function() {
      btPageData.eventData = {
        eventName	: 'ttpMyAccountModalDisplayed'
      };
      _satellite.track('pageEvent');
    });

    window.addEventListener('ttpHeaderModalAccepted', function() {
      btPageData.eventData = {
        eventName	: 'ttpHeaderModalAccepted'
      };
      _satellite.track('pageEvent');
    });

    // regular email events that work on React as well
    // gigya event listener
    setTimeout(function() {
      try {
        gigya.socialize.addEventHandlers({
          onLogin: function(e) {
            btPageData.eventData = {
              eventName		: 'Social Login',
              socialNetwork	: e.provider
            };

            _satellite.track('pageEvent');
          }
        });
      } catch(ex) {}

      if (window.jQuery) {
        $(window).on('emailSignupComplete', function(e) {
          btPageData.eventData = {
            eventName: 'Email Signup Completed'
          };
          _satellite.track('pageEvent');
          _satellite.track('emailSignupComplete');
        });
      }
    }, 5000);
  },

  getProductList				: function(data) {
    if (!data) {
      return {};
    }

    var list = {
      refinements: [],
      sortBy: data.sort || '',
      pageNumber: parseInt(data.page || '0') + 1,
      perPage: parseInt(data.fetchedProductCount || '0'),
      totalResults: parseInt(data.totalProductCount || data.fetchedProductCount || '0'),
      items: []
    };
    
    if (data.sortParamSource && data.algorithm) {
	    list.dsaAlgorithm = data.algorithm + ':' + data.sortParamSource;
    }

    if (data.filter) {
      for (var x in data.filter) {
        var obj = {};
        obj[x.replace(/refinement/gi, '').toLowerCase()] = data.filter[x];
        list.refinements.push(obj);
      }
    }

    if (data.products) {
      for (var i=0; i<data.products.length; i++) {
        list.items.push({
          id: !data.products[i] ? '' : data.products[i].productId
        });
      }
    }

    if (btPageData.page.type === 'Search') {
      list.search = {
        keyword: (_satellite.getVar('Query_q') || '').replace(/\+/g, ' ')
      };
    }

    return list;
  },
  
  getCustomerData				: function(pageData) {
	var vis = {};
	try {
      vis.customerId = JSON.parse(localStorage.getItem('headerDynamicDataInfo')).visitor.customerId;
    } catch(ex) {}
    var uci= localStorage.getItem('expCustId');
    var custLoyaltyData = JSON.parse(localStorage.getItem('loyaltyWarmStateData'));
    var custLoyaltyId = custLoyaltyData ? custLoyaltyData.loyaltyNumber : '';
    console.log(pageData);
    if (pageData.user && pageData.user.customer) {
      var cust;
      if (pageData.user.customer.loginStatus) {
        cust = pageData.user.customer;
      } else if (pageData.user.customer.customerInfo && pageData.user.customer.customerInfo.loginStatus) {
        cust = pageData.user.customer.customerInfo;
      }
      if (cust) {
        vis.firstName = cust.firstName || '';
        vis.lastName = cust.lastName || '';
        vis.emailAddress = (cust.emailAddress || '').toLowerCase();
        vis.loginStatus = 'Logged In';
        vis.loyaltyId = cust.loyaltyNumber || custLoyaltyId || '';
        vis.preference = cust.gender ? cust.gender.charAt(0).toUpperCase() : '';
        vis.rewardsTier = cust.tierName || '';
        vis.cardHolder = cust.nextCreditCardHolder ? 'true' : 'false';
        vis.pointsBalance = cust.pointsBalance || '';
        vis.pointsToNextTier = cust.pointsToNextTier || '';
        vis.pointsTowardsAListStatus = cust.pointsTowardsAListStatus || '';
        vis.rewardAmountInCentsForAListReward = cust.rewardAmountInCentsForAListReward || '';
        vis.rewardAmountInCentsForNextReward = cust.rewardAmountInCentsForNextReward || '';
        vis.totalPointsForAListReward = cust.totalPointsForAListReward || '';
        vis.totalPointsForNextReward = cust.totalPointsForNextReward || '';
        vis.associateLogin = cust.associateLogin ? 'true' : 'false';
        vis.phoneNumber = cust.phoneNumber ? cust.phoneNumber : '';
        vis.customerId = cust.id;
        vis.uci = uci;
        if (cust.loyaltyStatus && cust.loyaltyStatus === 'MEMBER_ACTIVATED') {
          vis.loyaltyStatus = 'Activated';
          vis.member = 'Y';
        } else {
          vis.loyaltyStatus = cust.loyaltyStatus || '';
          vis.member = 'N';
        }
      }
    } else {
      vis.loginStatus = 'Guest';
      vis.loyaltyId = 'N.A.';
      vis.loyaltyStatus = 'Guest';
      vis.member = 'N';
      vis.preference = 'F';
    }
    
    if (vis.emailAddress) {
	  try {
	    const hashValue = val => crypto.subtle.digest('SHA-1', new TextEncoder('utf-8').encode(val)) .then(h => {
          let hexes = [], view = new DataView(h);
          for (let i = 0; i < view.byteLength; i += 4) {
            hexes.push(('00000000' + view.getUint32(i).toString(16)).slice(-8));
          }
          return hexes.join('');
        });
        
        hashValue(vis.emailAddress).then(shaVal => {vis.emailAddressSHA1 = shaVal});
      } catch(ex) {
	    // browser doesn't support crypto
      }
    }
    
    return vis;
  },

  getProductData				: function(prods, fullData) {
    if (!prods) {
      return '';
    }
    var prodList = [];
    function _getTrueId(__p) {
      if ( typeof window._expr_debug === 'function' ) window._expr_debug( '[ANALYTICS] _getTrueId', __p );

      if (!__p || (__p && !__p.productId)) return '';
      var __id = __p.productId;

      // Final Sale ID
      if ( __id.indexOf( '_fv' ) !== -1 ) __id = __id.substr( 0, __id.indexOf( '_fv' ) );
      if ( __id.substr(-1) === 'F' ) __id = __id.substr( 0, ( __id.length - 1 ) );

      // CT ID
      if (__id.indexOf('-') > -1) {
        __id = __p.productSlug;
      }

      // For clearance indicator
      if ( __id.substr(-1) !== 'C' ) {
        var _price = __p.salePrice || ( __p.sku && __p.sku.displayPrice ) || '';
        if ( _price && _price.substr(-1) === '9' || _price.substr(-1) === '7') {
          __id = __id + 'C';
        }
      }

      return __id;
    }
    if (prods && prods.length > 0) {
      for (var i=0; i<prods.length; i++) {
        if (!prods[i]) continue;
        var thisProd = prods[i].product || prods[i];
        console.log("for prods.product.length", thisProd)
        var prod = {
          averageRating			: '',
          category				: '',
          colorCode				: '',
          discount				: 0,
          gender				: thisProd.gender || '',
          id					: _getTrueId(thisProd),
          inStock				: true,
          isBopis				: false,
          isLowInventory		: false,
          isMarketplaceProduct	: false,
          name					: thisProd.name || '',
          originalPrice			: thisProd.listPrice ? parseFloat(thisProd.listPrice.substring(1)) : '',
          price					: thisProd.salePrice ? parseFloat(thisProd.salePrice.substring(1)) : '',
          promoMessage			: thisProd.promoMessage || '',
          quantity				: thisProd.quantity || prods[i].quantity || 1,
          recommendationType	: thisProd.recsAlgorithm || '',
          sellerId				: '',
          sku					: '',
          totalReviews			: '',
          url					: thisProd.productURL || '',
        };
		console.log(prods);
		console.log(fullData);
        if (fullData.recsAlgorithm || fullData.recsLocation || fullData.recsResource) {
	        prod.recommendationType = (fullData.recsAlgorithm || 'null') + ':' + (fullData.recsLocation || 'null') + ':' + (fullData.recsResource || 'null');
	        if (fullData.recsReferring) {
		        prod.referringId = fullData.recsReferring;
	        }
        } else if (prod.recommendationType && btPageData.page.lastType && btPageData.page.lastType === 'Product' ) {
          prod.referringId = (function() {
            if (typeof btPageData.items !== 'undefined'
              && btPageData.items.length
              && btPageData.items[0]
            ) return btPageData.items[0].id;

            return null;
          })();
        }

        var discount = (function() {
          if (prods[i].itemPromotionDiscount && prods[i].itemPromotionDiscount.displayAmount) {
            return parseFloat(prods[i].itemPromotionDiscount.displayAmount.substring(1));
          }
          return 0.00;
        })();

        if (thisProd.colorSlices) {
          for (var j=0; j<thisProd.colorSlices.length; j++) {
            if (thisProd.colorSlices[j].skus) {
              for (var k=0; k<thisProd.colorSlices[j].skus.length; k++) {
                if (thisProd.colorSlices[j].skus[k].marketPlaceSku) {
                  prod.isMarketplaceProduct = true;
                  break;
                }
              }
              if (prod.isMarketplaceProduct) {
                break;
              }
            }
          }
        }
        if (thisProd.miraklData && thisProd.miraklData.sellerId) {
          prod.sellerId = thisProd.miraklData.sellerId;
        }
        /*
        var colorMatch = '/color/', path = document.location.pathname;
        if (path.indexOf(colorMatch) > -1) {
          path = path.substring(path.indexOf(colorMatch) + colorMatch.length);
          path = decodeURIComponent(path.substring(path, path.indexOf('/')));
          if (thisProd.colorSlices) {
            for (var j=0; j<thisProd.colorSlices.length; j++) {
              if (path === thisProd.colorSlices[j].color) {
                prod.swatchColor = thisProd.colorSlices[j].ipColorCode;
                break;
              }
            }
          }
        }
        */
        if (fullData.selectedColorName) {
          prod.swatchColor = fullData.selectedColorName.toLowerCase();
        }
        
        if (thisProd.productImage) {
	      prod.imageUrl = thisProd.productImage;
        }
        if (thisProd.productURL) {
	      prod.url = thisProd.productURL;
        }
        
        if (thisProd.productInventory) {
          prod.inventoryCount = thisProd.productInventory;
          if (thisProd.productInventory < 11) {
            prod.isLowInventory = true;
          }
          if (thisProd.productInventory < 1) {
            prod.inStock = false;
          }
        } else {
          prod.inStock = false;
          prod.inventoryCount = 0;
        }
        if (prods[i].price) {
          prod.price = prods[i].price.amount;
        }
        if (prods[i].isStorePickup || (prods[i].fulfillmentMethod && prods[i].fulfillmentMethod.toLowerCase() === 'bopis')) {
          prod.isBopis = true;
        }
        if (parseFloat(prod.price) == 0.00 && (parseFloat(prod.originalPrice) !== parseFloat(discount))) {
          prod.price = prod.originalPrice;
        }
        if (prod.gender) {
          prod.gender = prod.gender.charAt(0).toUpperCase() + prod.gender.substring(1) + 's';
        } else if (window.btPageData && btPageData.page && btPageData.page.name) {
          if (btPageData.page.name.indexOf('Women') > -1) {
            prod.gender = 'Womens';
          } else if (btPageData.page.name.indexOf('Men') > -1) {
            prod.gender = 'Mens';
          }
        }
        prod.category = prod.category.replace(prod.id + '/', '');
        if (fullData && fullData.feedback) {
          try {
            var rev = fullData.feedback.ratings ? fullData.feedback.ratings : {};
            prod.averageRating = rev ? rev.metrics.overall : '';
            prod.totalReviews = rev ? rev.count : '';
            prod.reviewRank = '';
          }catch(ex){};
        }

        // For the current version of app_express, a sku is selected only when size and color
        // are selected. Meaning the selected skuID is not available by the time REACTReady is fired
        if (fullData && fullData.colors && fullData.selectedColorIndex && fullData.selectedSkuIndex) {
          prod.sku = fullData.colors[fullData.selectedColorIndex].skus[fullData.selectedSkuIndex].skuId;
        } else if (thisProd.sku) {
          prod.sku = thisProd.sku.skuId;
          prod.colorCode = thisProd.sku.colorCode;
        }


        if (thisProd.skuId) {
          prod.sku = thisProd.skuId;
        }

        if (prods[i].giftWrapSelected) {
          prod.hasGiftBox = true;
        } else {
          prod.hasGiftBox = false;
        }

        if (btPageData.page.type == 'Product') {
          try {
            var link = thisProd.breadCrumbCategory ? thisProd.breadCrumbCategory.links[0] : {};
            var href = link ? link.href : '';
            prod.category = thisProd.productURL.substring(thisProd.productURL.indexOf(thisProd.productId) + thisProd.productId.length + 1);
            prod.categoryUrl = href ? 'https://www.express.com/' + href.split('/').slice(3).join('/') : '';
            prod.categoryName = thisProd.breadCrumbCategory ? thisProd.breadCrumbCategory.categoryName : '';
            prod.displayPrice = thisProd.colorSlices[0];
            prod.isEnsemble = thisProd.expressProductType === 'ensemble' ? 'true' : 'false',
              prod.ensembleId = href ? href.split('/').pop() : '';
            prod.editionProduct = thisProd.productDetail === 'EDITION'? true : false;
            prod.isMarketplaceProduct = thisProd.marketPlaceProduct;
            prod.EFOProduct = thisProd.EFOProduct;
            // In CT h1CategoryName is not returned
            prod.productType = thisProd.breadCrumbCategory ? thisProd.breadCrumbCategory.h1CategoryName : '';
            prod.discountMessage = $('._3bJC span').text() || thisProd.promoMessage;
          } catch(ex) {
          }
          if (!prod.category && btPageData.items && btPageData.items.length) {
            // find product in btPageData.items
            for (var j=0; j<btPageData.items.length; j++) {
              if (btPageData.items[j].id == prod.id) {
                prod.category = btPageData.items[j].category || '';
                prod.categoryUrl = btPageData.items[j].categoryUrl || '';
                prod.categoryName = btPageData.items[j].categoryName || '';
                prod.displayPrice = btPageData.items[j].displayPrice || '';
                prod.isEnsemble = btPageData.items[j].isEnsemble || '';
                prod.ensembleId = btPageData.items[j].ensembleId || '';
                prod.productType = btPageData.items[j].productType || '';
                prod.discountMessage = btPageData.items[j].discountMessage || '';
                break;
              }
            }
          }
        }
        prodList.push(prod);
      }

      return prodList;
    }
  },

  // handle all CSS selector events the same way
  trackLink					: function(name, el) {
    if (!(name || el)) {
      return;
    }

    var $el, $;
    if (window.jQuery) {
      $ = window.jQuery;
      $el = $(el);
    }

    btPageData.eventData = {};

    switch (name) {
      case 'Social Login Start':
        if ($el) {
          btPageData.eventData.socialNetwork = $el.attr('aria-label');
        }
        break;
      case 'Size Chart':
      case 'Color Change':
      case 'Alternate Image':
      case 'Accordion Expand':
      case 'Product Carousel':
      case 'Image Zoom':
        var id = btPageData.expressViewCurrentId;
        if (!id) {
          if (btPageData.items && btPageData.items[0] && btPageData.items[0].id) {
            id = btPageData.items[0].id;
          } else {
            id = '';
          }
        }
        var type = _satellite.getVar('Page_Type') == 'Product' ? 'Product' : 'Express View';
        var evt = type + ' : ' + id + ' : ' + name;

        if (name == 'Color Change') {
          if ($el) {
            if ($el.hasClass('product-color')) {
              var color = $el.attr('aria-label');
              color = color.substring(color.indexOf('color ')).replace('color ', '');
              evt += ' : ' + color;
            } else {
              evt += ' : ' + $el.find('span.colorSwatchNameCode').text();
            }
          }
        } else if (name == 'Alternate Image') {
          if ($el) {
            var pos = $el.attr('class').match(/carousel__dot--(\d)+/)[0].replace('carousel__dot--', '');
            evt += pos ? ' : ' + (parseInt(pos) + 1) : '';
          }
        } else if (name == 'Product Carousel') {
          if ($el) {
            evt += (' : ') + ($el.hasClass('carousel__next-button') || $el.hasClass('slick-next') ? 'Next' : 'Previous');
          }
        } else if (name == 'Accordion Expand') {
          evt += $el ? ' : ' + $el.text() : '';
        } else if (name == 'Image Zoom') {
          var $parent = $el ? $el.parent() : '';
          if ($parent) {
            $parent.find('div.carousel__image').each(function(i) {
              if ($(this) == $el) {
                evt += ' : ' + (i + 1);
                return false;
              }
            });
          }
        }
        btPageData.eventData.productInteractionName = evt;
        break;
    }

    btPageData.eventData.eventName = name;

    _satellite.track('pageEvent');
  },
  // used to bind legacy jQuery event handlers
  bindJQueryEventHandlers	: function($) {
    // express view
    var ev = ['expressView', 'storyExpressView'];
    for (var i=0; i<ev.length; i++) {
      $(window).on(ev[i], function(e, d) {
        btPageData.expressViewCurrentId = d.items[0].id;
        btPageData.eventData = {
          eventName: 'Express View',
          items: d.items
        };

        if (d.recommendationType) {
          btPageData.eventData.recommendationType = d.recommendationType;
        }

        _satellite.track('pageEvent');
        _satellite.track('expressView');
        _satellite.track('productView');
      });
    }

    // cart add
    var ca = ['cartItemAdd', 'storyCartItemAdd'];
    for (var i=0; i<ca.length; i++) {
      $(window).on(ca[i], function(e, d) {
        btPageData.eventData = {
          eventName: 'Cart Add',
          items: d.items
        };

        _satellite.track('pageEvent');
        _satellite.track('cartAdd');
      });
    }
    // Handle bag peek view on AEM Pages
    window.addEventListener('ViewedBagPeek', function() {
      btPageData.eventData = {
        eventName	: 'Bag Peek View'
      };
      _satellite.track('pageEvent');
      _satellite.track('ViewedBagPeek');
    });
    // color code tracking
    $(window).on('swatchSelected', function(e, d) {
      if (d.items && d.interactionName) {
        btPageData.eventData = {
          eventName: 'Swatch Selected',
          swatchColor: d.items[0].id + '|' + d.items[0].colorCode.toLowerCase() + '|' + d.interactionName.toLowerCase()
        };
      }
      _satellite.track('pageEvent');
    });

    // refinement, sort by, pagination, etc.
    $(window).on('updateProductList', function(e, d) {
      if (d.productList) {
        window.btPageData.productList = window.btPageData.productList || {};
        if (d.productList.refinements) {
          btPageData.productList.refinements = d.productList.refinements;
        }
        if (d.productList.sortBy) {
          btPageData.productList.sortBy = d.productList.sortBy;
        }
        if (d.productList.totalResults) {
          btPageData.productList.totalResults = d.productList.totalResults;
        }
        if (d.productList.pageNumber) {
          btPageData.productList.pageNumber = d.productList.pageNumber;
        }
        if (d.productList.perPage) {
          btPageData.productList.perPage = d.productList.perPage;
        }
        if (d.productList.gridStyleOld) {
          btPageData.productList.gridStyleOld = d.productList.gridStyleOld;
        }
        if (d.productList.gridStyle) {
          btPageData.productList.gridStyle = d.productList.gridStyle;
        }

        btPageData.eventData = {
          eventName: 'Update Product List'
        };
      }

      _satellite.track('pageView');
    });
    //Checkout page WOLPS tracking
    $(window).on('wolpsInvite', function(e) {
      btPageData.eventData = {
        eventName		: 'WOLPS Invite',
        virtualPageView	: true
      };

      _satellite.track('pageView');
    });

    $(window).on('wolpsApplication', function(e) {
      btPageData.eventData = {
        eventName		: 'WOLPS Application Start',
        virtualPageView	: true
      };

      _satellite.track('pageView');
    });

    $(window).on('wolpsNotification', function(e) {
      btPageData.eventData = {
        eventName			: 'WOLPS Application Submitted',
        virtualPageView		: true,
        applicationStatus	: d.applicationStatus || 'no status provided'
      };

      _satellite.track('pageView');
      _satellite.track('enccApplicationSubmitted');
    });

    //EFO Email Sign-up
    $(window).on('outletEmailSignup', function(e) {
      btPageData.eventData = {
        eventName: 'EFO Email Signup Completed'
      };
      _satellite.track('pageEvent');
      _satellite.track('outletEmailSignup');
    });

    // Bluecore email events
    window.addEventListener('express_MarketingEvent', function(e) {
      if (e.detail && e.detail.eventName && e.detail.modalName)
        btPageData.eventData = {
          eventName			: 'Bluecore ' + (e.detail.ctaName ? 'Submit Form' : 'View Overlay'),
          bluecoreModalName	: e.detail.modalName + (e.detail.ctaName ? ':' + e.detail.ctaName : '')
        };
      _satellite.track('pageEvent');
    });
  },
  
  getParam: function (name, url) {
    if (!url) {
      url = window.location.href;
    } else if (url.indexOf('?') == -1) {
      url = '?' + url;
    }
    url = decodeURIComponent(url);
    var regexS = '[\\?&]+' + name + '=([^&#]*)';
    var regex = new RegExp(regexS, 'i');
    var results = regex.exec(url);
    if (results === null) {
	  return '';
    } else {
      return results[1].replace(/^[ \t]+|[ \t]+$/, '');
    }
  }
};

