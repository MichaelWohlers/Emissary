  sswRun(function(){
    var intervalID = setInterval(function(){
      if(ssw('.mobile-nav-panel #customer_login_link').length){
        ssw('.mobile-nav-panel #customer_login_link').click(function() {
          ssw('.mobile-nav-close').click();
