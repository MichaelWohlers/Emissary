            <script>
                $(document).ready(function() {
                    $(".swiper-slide").removeClass("int-hide-on-load");
                    var mySlider;
                    mySlider = $('.swiper-container-global-header').bxSlider({
                        auto: 'true',
                        mode: 'vertical',
                        pager: false,
                        controls: false,
                        responsive: true,
                        pause: 4000,
                        autoHover: true
                    });


                    $(window).resize(function() {
                        mySlider.reloadSlider();
                    });

                    const observer = new MutationObserver(function(mutations) {

                        mutations.forEach(function(mutation) {
                            if (mutation.attributeName === "style") {
                                mySlider.reloadSlider();
                            }
                        })
                    });

                    const animbannergh = document.querySelector("div.header-banner-home");
                    observer.observe(animbannergh, {
                        childList: false,
                        attributes: true
                    })
                });
            </script>

        </div>
    </div>

</div>


