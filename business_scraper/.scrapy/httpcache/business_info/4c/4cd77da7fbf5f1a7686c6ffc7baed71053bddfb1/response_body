<!--Free Shipping Footer CSS -->
<style>
    .footer_free-shipping,
    .footer_no-free-shipping {
        display: none;
    }

    .unhide_block {
        display: block;
    }

    .hide-footer {
        display: none !important;
    }

    .unhide-footer {
        display: block;
    }
</style>
<!--Free Shipping Footer Script -->
<script>
    if (footerFS == 'Y') {
        $(".footer_free-shipping").addClass("unhide_block");
    } else {
        $(".footer_no-free-shipping").addClass("unhide_block");
    }


    //Hide Featured Footer for HP Laura, Petites and Plus
    $(document).ready(function() {
        if ($(".hp-services-wrapper")[0]) {
            $(".fullbackprefooter").addClass("hide-footer");
        } else {
            $(".fullbackprefooter").addClass("unhide-footer");
        }
    });
</script>


