    // MightyScroll
    $.fn.scrollFun = function() {
        var headerHeight = $("div#lauraHeader").height();
        $(this).click(function(e) {
            var h = $(this).attr('href'),
                target;

            if (h.charAt(0) == '#' && h.length > 1 && (target = $(h)).length > 0) {
                var pos = Math.max(target.offset().top - 120, 0);
                e.preventDefault();
                $('body,html').animate({
                    scrollTop: pos
                }, 'slow', 'swing');
            }
        });
    };
    $('.smoothscroll').scrollFun();


