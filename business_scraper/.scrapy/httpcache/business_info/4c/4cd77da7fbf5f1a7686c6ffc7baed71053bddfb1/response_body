<script>
    $(document).ready(function() {
        if ("/en/shopping-bag/" === window.location.pathname) {
            window.setInterval(
                function() {
                    if ($('.opc-brand--laura #cart-table .basket-item .product__price--adjusted.text-alignment .product__price--adjusted').text().length > 0) { // Checking the text inside a div
                        if ($('.product__price.product__price--adjusted').text().indexOf('- &nbsp')) {
                            $('.opc-brand--laura #cart-table .basket-item .product__price--adjusted.text-alignment .product__price--adjusted:contains("- &nbsp")').css("display", "none");
                        }

                    }
                    if ($('.product__price.product__price--adjusted').text().length > 0) { // Checking the text inside a div

                        // Condition to check the  text match
                        if ($('.product__price.product__price--adjusted').text().indexOf('- &nbsp')) {
                            $('.opc-brand--laura .opc-mini-summary .list .product__price--adjusted div:contains("- &nbsp")').css("display", "none");
                        }
                    }
                }, 50);
        }
    });
</script>
