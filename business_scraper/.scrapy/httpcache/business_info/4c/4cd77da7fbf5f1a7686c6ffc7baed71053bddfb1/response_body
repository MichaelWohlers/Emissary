<script type="text/javascript">
function setLanguageCookie(lang){
var thousandDays = 1000;
var d = new Date();
d.setTime(d.getTime() + (thousandDays*24*60*60*1000));
var expires = "expires="+ d.toUTCString();
document.cookie = "user-language=" + lang + "; " + expires + "; path=/";
}
</script>


