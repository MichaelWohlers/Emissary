	<script>

		
      $(function(){

        $('.mobile #menuButton').on('click',function(){
          $('body').css({'overflow':'hidden','position':'relative'});
          $('#menuButton').addClass('open');
          $('#headerNavWrapper').addClass('visible open').animate({'left':'0'}).css({'height':'100vh'});
          $('.shadow').removeClass('hidden').show().css({'visibility':'visible'}).animate({'opacity':'1'})
        });
        $('.navBarCloseWrapper a, .shadow').on('click',function(){
          $('body').css({'overflow':'','position':''});
          $('#headerNavWrapper').animate({
            'left':'-100%'
          },function(){
            $('#headerNavWrapper').removeClass('visible open').css({'height':''})
          });
          $('.shadow').animate({
            'opacity':'0'
          },function(){
            $('.shadow').addClass('hidden').css({'visibility':'hidden'})
          });
        });
            
        $('.plusIcon').on('click',function(){
          $(this).closest('.dropDownColumn').removeClass('close').addClass('open')
          $(this).parent().next().css({'visibility':'visible','display':'block','height':'auto'}).animate({'opacity':'1'});
        })

        $('.minusIcon').on('click',function(){
          $(this).parent().next().animate({'opacity':'0'});
          $(this).parent().next().css({'visibility':'hidden','display':'none','height':'0'})
          $(this).closest('.dropDownColumn').removeClass('open').addClass('close');
        });

        $('.footer-links-col.expand').on('click',function(){
          var $linkCol = $(this);
          var $footerLink = $(this).find('.footer-link');
          var $heading = $linkCol.find('h5');
          var openClass = 'open';

          var $this = $heading;
          var $parent = $linkCol;

          $linkCol.removeAttr('style');

          if($parent.hasClass(openClass)) {

            $linkCol.removeClass(openClass);

          } else {

            var height = 0;

            $this.parent($linkCol).find('> *').each(function() {
              height = height+$(this).outerHeight();
            });

            $this.parent($linkCol).height(height);

            $($linkCol).removeClass(openClass);
            $parent.addClass(openClass);

          }
        })

      // }
    });

		function visitStorePage(add) {
			var a = 'https://stores.calvinklein.us/' + add[0].toLowerCase() + '/' + add[1].toLowerCase().replace(/ /g,'-') + '/' + add[2];
			window.open(a,'_self');
		
		}
	</script>
