					$('.storeHours li').each(function(){
						// var a = $(this).html().replace(/:00/g,'').replace(/-/g,' - ');
						var a = $(this).html().replace(/:00/g,'').replace(/-/g, ' - ');
						$(this).html(a);
					})
		
					
	                var date = new Date();
	                var day = date.getDay();
	                switch (day) {
                    case 0:
                        $('.storeHours-item:last-of-type').addClass('bold');
                        $('.storeHours-item.bold span:first-of-type').html('Today');
                        break;
                    case 1:
                        $('.storeHours-item:first-of-type').addClass('bold');
                        $('.storeHours-item.bold span:first-of-type').html('Today');
                        break;
                    case 2:
                        $('.storeHours-item:nth-of-type(2)').addClass('bold');
