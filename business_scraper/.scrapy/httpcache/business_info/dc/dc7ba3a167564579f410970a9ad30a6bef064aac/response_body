	            var date = new Date();
				var today = date.getDay();

                for (var index = 0; index < W2GI.collection.poi.length; index++) {
                	if(W2GI.collection.poi[index].sun_hours.toLowerCase() == 'closed' && W2GI.collection.poi[index].mon_hours.toLowerCase() == 'closed' && W2GI.collection.poi[index].tues_hours.toLowerCase() == 'closed' && W2GI.collection.poi[index].wed_hours.toLowerCase() == 'closed' && W2GI.collection.poi[index].thurs_hours.toLowerCase() == 'closed' && W2GI.collection.poi[index].fri_hours.toLowerCase() == 'closed' && W2GI.collection.poi[index].sat_hours.toLowerCase() == 'closed') {
                                  W2GI.collection.poi[index].closehours = 'Closed';
	                } else {

	                    switch(today) {
	                        case 0:
	                        	W2GI.collection.poi[index].closehours = openClosed(W2GI.collection.poi[index].sun_hours,W2GI.collection.poi[index].mon_hours);
	                            //W2GI.collection.poi[index].TODAYSHOURS = "Sunday " + W2GI.collection.poi[index].sun_hours;
	                            
	                        break;
	                        case 1:
	                           
	                         
	                                W2GI.collection.poi[index].closehours = openClosed(W2GI.collection.poi[index].mon_hours,W2GI.collection.poi[index].tues_hours);
	                              //  W2GI.collection.poi[index].TODAYSHOURS = "Monday " + W2GI.collection.poi[index].mondayhours;
	                          
	                        break;
	                        case 2:
	                          	console.log('tues_hours = ' +  W2GI.collection.poi[index].tues_hours);
	                                W2GI.collection.poi[index].closehours = openClosed(W2GI.collection.poi[index].tues_hours,W2GI.collection.poi[index].wed_hours);

	                               // W2GI.collection.poi[index].TODAYSHOURS = "Tuesday " + W2GI.collection.poi[index].tuesdayhours;
	                            
	                        break;
	                        case 3:
	                           
	                            	W2GI.collection.poi[index].closehours = openClosed(W2GI.collection.poi[index].wed_hours,W2GI.collection.poi[index].thurs_hours);
	                               // W2GI.collection.poi[index].TODAYSHOURS = "Wednesday " + W2GI.collection.poi[index].wednesdayhours;
	                           
	                        break;
	                        case 4:
	                            
	                            	W2GI.collection.poi[index].closehours = openClosed(W2GI.collection.poi[index].thurs_hours,W2GI.collection.poi[index].fri_hours);
	                               // W2GI.collection.poi[index].TODAYSHOURS = "Thursday " + W2GI.collection.poi[index].thursdayhours;
	                            
	                        break;
	                        case 5:
	                           	W2GI.collection.poi[index].closehours = openClosed(W2GI.collection.poi[index].fri_hours,W2GI.collection.poi[index].sat_hours);
	                               // W2GI.collection.poi[index].TODAYSHOURS = "Friday " + W2GI.collection.poi[index].fridayhours;
	                           
	                        break;
	                        case 6:
	                         
	                            	W2GI.collection.poi[index].closehours = openClosed(W2GI.collection.poi[index].sat_hours,W2GI.collection.poi[index].sun_hours);
	                              //  W2GI.collection.poi[index].TODAYSHOURS = "Saturday " + W2GI.collection.poi[index].saturdayhours;
	                           
	                        break;
	                    }
	                  
	                }
                }

                 function openClosed(today, tomorrow){
                	var date = new Date(),
                    hour = date.getHours();
                    if(today.toLowerCase() != 'closed'){
                        var openHours = today.split('-'),
                        openTime = parseInt(openHours[0].slice(0,1)),
                        openTimeShow = openHours[0].replace(':00',''),
                        closeTime = parseInt(openHours[1]) + 12,
                        closeTimeShow = openHours[0].replace(':00','') + ' - ' + openHours[1].replace(':00','');
                    } else {

                    }
                    if(tomorrow.toLowerCase() != 'closed'){
                        var closeTime2 = tomorrow.split('-')[0];
                    } else {

                    }
                   if(openTime <= hour && hour <= closeTime){
                    	console.log('Open until ' + closeTime)
                    	console.log('Open until ' + closeTimeShow)
                    // $('.nearby_hours').addClass('open');
                    	return 'Open today ' + closeTimeShow
	                } else if(openTime > hour) {
	                    // console.log('Closed until ' + closeTime)
	                    return 'Closed until ' + openTimeShow + ' tomorrow'
	                } else if(hour > closeTime) {
	                    // console.log('Closed until ' + openTime2)
	                    return 'Closed until ' + closeTime2
	                }


	            }

