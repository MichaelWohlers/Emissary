	 <script>
      $(function(){
        $(window).on('resize',function(){
            switchClass();
            checkHeadingHeight();
        })
      });

		function checkHeadingHeight() {
      		if($(window).width() > 801) {
      			if($('h1').height() > 42){
      				$('.covid').css('margin-top','0');
      			}
      		} else {
      			if($('.covid').css('margin-top')){
      				$('.covid').css('margin-top','');
      			}
      		}
      	}
    </script>

