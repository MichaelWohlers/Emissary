	                    $('#bubble_hours').html($('#all_hours_table').html());
	                    $("#map form.bubble_form_2").on("submit", function (evt) {
	                        evt.preventDefault();
	                    });
	                    //$('#map .bubble_form_2 .bubble_input')[0].focus();

