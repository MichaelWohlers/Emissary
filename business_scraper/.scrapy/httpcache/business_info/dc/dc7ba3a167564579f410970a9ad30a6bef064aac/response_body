	            	            function generate_directions (startaddress, endaddress) {

	                endaddress = endaddress.replace("#", "poundsign");

	                if (bubbleValidate(startaddress)) {
	                    $('#directions_url').data('url', '' + startaddress + '&addressline=' + '29.8287208,-97.9803606' + '&addresslineDisplay=' + endaddress);
	                    ace.util.alertExternal($('#directions_url'));
	                    //$('#directions_url').click();
	                    //performClick($('#directions_url'));
	                }
	            }

	            function bubbleValidate (bubbleInput) {
	                if (bubbleInput.length > 0 && bubbleInput.length < 5 && !isNaN(bubbleInput)) {
	                    ace.util.alert("Please enter a valid ZIP code.");
	                    return false;
	                }

	                if (bubbleInput.length == 0) {
	                    ace.util.alert("Please enter a starting address.");
	                    return false;
	                }

	                return true;
	            }

	            function performClick (node) {
	                if ((navigator.userAgent.match(/Safari/i))) {
	                    var evObj = document.createEvent('MouseEvents');
	                    evObj.initMouseEvent('click', true, true, window);
	                    node.dispatchEvent(evObj);
	                } else {
	                    node.click();
	                }
	            }



	            function tConvert (time) {
	               // Check correct time format and split into components
	               time = time.toString ().match (/^([01]\d|2[0-3])(:)([0-5]\d)(:[0-5]\d)?$/) || [time];

	                if (time.length > 1) { // If time format correct
	                  time = time.slice (1);  // Remove full string match value
	                  time[5] = +time[0] < 12 ? ' AM' : ' PM'; // Set AM/PM
	                  time[0] = +time[0] % 12 || 12; // Adjust hours
	                }
	                return time.join (''); // return adjusted time or original string
	              }


					window.addEventListener('DOMContentLoaded', function() {
					    var collectionPoi = W2GI.collection.poi;
					    var staticMap = document.getElementById('staticMap');
					    var mapUrl;
					    // if(window.innerWidth > 801) {
					    	//var size = '959x601';
					    // } else {
					    // 	var size = '801x375';
					    // }
					    if (collectionPoi.length > 1) {
					        var emptyMarkerArray = [];
					            for (i = 1; i < collectionPoi.length; i++) {
					            emptyMarkerArray.push('&markers=icon:https://stores.calvinklein.us/images/local/map-pin-local.'+[i]+'.png|' + collectionPoi[i].latitude + ',' + collectionPoi[i].longitude);
					        }
					        var populatedMarkerArray = emptyMarkerArray.join('');
					        mapUrl = 'https://maps.googleapis.com/maps/api/staticmap?language=en&key=AIzaSyC_poY9u57toWlNmTpIQmEjM7dw2zUJjh4&channel=calvinklein&autoscale=1&size=959x601&maptype=roadmap&format=png&visual_refresh=true&markers=icon:https://stores.calvinklein.us/images/local/map-pin-local.png|29.8287208,-97.9803606' + populatedMarkerArray;
					        staticMap.style.background = "center url('"+mapUrl+"')";
					        // populateNearbyText(collectionPoi);
					        } else {
					      mapUrl = 'https://maps.googleapis.com/maps/api/staticmap?language=en&key=AIzaSyC_poY9u57toWlNmTpIQmEjM7dw2zUJjh4&channel=calvinklein&center=29.8287208,-97.9803606&zoom=14&scale=1&size=959x601&maptype=roadmap&format=png&visual_refresh=true&markers=icon:https://hosted.where2getit.com/calvinklein/images/local/map-pin-local-1.png%7C29.8287208,-97.9803606';
					        staticMap.style.background = "center url('"+mapUrl+"')";
					    }
