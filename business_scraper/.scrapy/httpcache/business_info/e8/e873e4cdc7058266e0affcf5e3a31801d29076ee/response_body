TJX.analytics.richRecsForceDisplayMode = "false";



TJX.analytics.rrRemovePlacement = function (name) {
	var index = TJX.analytics.placementTypes.indexOf(name);

	if (index > -1) {
		TJX.analytics.placementTypes.splice(index, 1);
	}
};



	if (existingPlacements.length) {
		TJX.analytics.csLocation = existingCsLocation;
		TJX.analytics.placementTypes = existingPlacements;
	}

	
