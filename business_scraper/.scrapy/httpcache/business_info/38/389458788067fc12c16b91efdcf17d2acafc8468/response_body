    <script type="text/javascript">
        var initialLocation;
        var map;
        var arrMarkers = [];
        var arrInfoWindows = [];
        var locations = [];
        var geocoder;
        var newyork = new google.maps.LatLng(40.69847032728747, -73.9514422416687);
        var burnsvilleMN = new google.maps.LatLng(44.797916, -93.278046);

        function mapInit(l, t) {

            geocoder = new google.maps.Geocoder();
            var centerCoord = new google.maps.LatLng(l, t);
            var mapOptions = {
                zoom: 9,
                center: centerCoord,
                mapTypeId: google.maps.MapTypeId.ROADMAP
            };
            map = new google.maps.Map(document.getElementById("map"), mapOptions);


        }
        function mapInitdefault() {

            geocoder = new google.maps.Geocoder();

            var mapOptions = {
                zoom: 1,

                mapTypeId: google.maps.MapTypeId.ROADMAP
            };
            map = new google.maps.Map(document.getElementById("map"), mapOptions);


        }

        $(function () {
            // initialize map (create markers, infowindows and list)
            // mapInit();

           var tmpurl = window.location.href.split('/');
            //alert(tmpurl.length);
            var tmp = tmpurl[tmpurl.length - 1];
            if (tmp != null) {
                $("#ContentPlaceHolder1_hdnStoreName").val(tmp);
            }
            callAjax();



            $(".listviewdata").on("click", function () {

                var i = $(this).attr("id").split('_')[1];

                var url = "https://www.IDoCollection.com/" + locations[i].url;

            $(this).attr("href", url);
            // this next line closes all open infowindows before opening the selected one
            for (x = 0; x < arrInfoWindows.length; x++) { arrInfoWindows[x].close(); }
            arrInfoWindows[i].open(map, arrMarkers[i]);
            // window.location = url;
        });

        // "live" bind click event
            $(document).on("click", '#list li', function () {

            var i = $(this).attr("rel");
            var url = "https://www.IDoCollection.com/index/" + locations[i].url;
               // alert(url);

            $(this).attr("href", url);
            // this next line closes all open infowindows before opening the selected one
            for (x = 0; x < arrInfoWindows.length; x++) { arrInfoWindows[x].close(); }
            arrInfoWindows[i].open(map, arrMarkers[i]);
            //window.location = url;
        });

            $(document).on('mouseenter', '#list li', function (e) {

            var i = $(this).attr("rel");

            // this next line closes all open infowindows before opening the selected one
            for (x = 0; x < arrInfoWindows.length; x++) { arrInfoWindows[x].close(); }
            arrInfoWindows[i].open(map, arrMarkers[i]);


        });

            $(document).on('mouseleave', '#list li', function (e) {

            var i = $(this).attr("rel");

            // this next line closes all open infowindows before opening the selected one
            for (x = 0; x < arrInfoWindows.length; x++) { arrInfoWindows[x].close(); }
            //arrInfoWindows[i].open(map, arrMarkers[i]);


        });

      $("#ContentPlaceHolder1_hypOpenPrintPopup").click(function () {

             var searchString = $("#ContentPlaceHolder1_hdnStoreName").val();
             openPrintRetailerPopup(searchString);
        });


    });
    function callAjax() {

        locations = [];
        arrMarkers = [];
        arrInfoWindows = [];
        var searchtxt = $("#ContentPlaceHolder1_hdnStoreName").val();
         searchtxt = searchtxt.split('?')[0];
        searchtxt = searchtxt.replace("#", "");

        $.ajax({
            type: "GET",
            contentType: "application/json; charset=utf-8",
            url: 'https://www.IDoCollection.com/GoogleMapUsAPI3JSonDATA.aspx?SearchText=' + searchtxt.trim(),
            dataType: "json",
            error: function (data, textStatus) {

                alert('No Search Results Available');
            },
            success: function (data, textStatus) {

                if (textStatus == "success") {

                    var myObject;
                    if (data.hasOwnProperty("d")) {
                        myObject = $.toJSON(data.d);
                    }
                    else { myObject = $.toJSON(data); }

                    var oServerJSON;
                    oServerJSON = eval("(" + myObject + ")");
                    //alert(oServerJSON);
                    $("#list").empty();
                    $("#tableListView").empty();
                    //$("#dList").empty();

                    for (var row = 0; row < oServerJSON.TABLE[0].ROW.length; row++) {
                        var location = new Object();
                        // location.distance = oServerJSON.TABLE[0].ROW[row].COL[0].DATA + " mi";
                        location.jewelerid = oServerJSON.TABLE[0].ROW[row].COL[0].DATA;
                        location.companydisplayname = oServerJSON.TABLE[0].ROW[row].COL[1].DATA.toUpperCase();
                        location.address1 = oServerJSON.TABLE[0].ROW[row].COL[2].DATA.toUpperCase();
                        location.address2 = oServerJSON.TABLE[0].ROW[row].COL[3].DATA.toUpperCase();
                        location.city = oServerJSON.TABLE[0].ROW[row].COL[4].DATA.toUpperCase();
                        location.statename = oServerJSON.TABLE[0].ROW[row].COL[5].DATA.toUpperCase();
                        location.statename = location.statename.split('-')[0];
                        location.emailid = oServerJSON.TABLE[0].ROW[row].COL[8].DATA;
                        location.zipcode = oServerJSON.TABLE[0].ROW[row].COL[9].DATA;
                        location.websiteurl = oServerJSON.TABLE[0].ROW[row].COL[10].DATA;
                        location.telphone1 = oServerJSON.TABLE[0].ROW[row].COL[11].DATA;
                        location.storehours1 = oServerJSON.TABLE[0].ROW[row].COL[12].DATA;
                        location.storehours2 = oServerJSON.TABLE[0].ROW[row].COL[13].DATA;
                        location.storehours3 = oServerJSON.TABLE[0].ROW[row].COL[14].DATA;
                        location.url = oServerJSON.TABLE[0].ROW[row].COL[6].DATA;
                        location.lat = oServerJSON.TABLE[0].ROW[row].COL[16].DATA;
                        location.lng = oServerJSON.TABLE[0].ROW[row].COL[17].DATA;
                        location.displayname = oServerJSON.TABLE[0].ROW[row].COL[1].DATA.toUpperCase();
                        //location.googleaddress = oServerJSON.TABLE[0].ROW[row].COL[19].DATA;
                        location.citystate = oServerJSON.TABLE[0].ROW[row].COL[5].DATA;
                        location.brachlocation = oServerJSON.TABLE[0].ROW[row].COL[18].DATA.toUpperCase();
                        if (row === 0) {
                            mapInit(location.lat, location.lng);
                            //$("#ctl00_ContentPlaceHolder1_txtZipCity").val(location.citystate);
                        }
                        locations[row] = location;
                        addLocation(location, row);
                        //zoomToBounds();


                    }

                    for (x = 0; x < arrInfoWindows.length; x++) { arrInfoWindows[x].close(); }
                    if (arrInfoWindows.length > 0) {
                        initialLocation = new google.maps.LatLng(locations[0].lat, locations[0].lng);
                        map.setCenter(initialLocation);
                    }

                    if (locations.length > 0) {
                        addTableListItem();

                    }
                    else {
                        alert('No Search Results Available');
                        mapInitdefault();
                        initialLocation = burnsvilleMN;
                        map.setCenter(initialLocation);
                    }
                }


            }
        });


    }
    function addLocation(location, i) {
        addListViewItem(location, i);
        addListViewItemNOMAP(location, i);


      
        $("#list").append('<div id="overview1"><ul class="WhereToBuyBox1" id="featurettes1"><li <a href="' + "https://www.IDoCollection.com/" + locations.url + '" rel="' + i + '">' + $("#listview_" + i).html() + '</a></li></ul></div>');
        codeAddress(location, i);
        if (i == 0) {
            $('.ps-scrollbar-x-rail').remove();
            $('.ps-scrollbar-y-rail').remove();
            // alert(document.querySelectorAll('.container11'));
            setTimeout(function () {
                [].forEach.call(document.querySelectorAll('.container11'), function (el) {
                    Ps.initialize(el);
                });
            }, 500)

        }

    }

    function codeAddress(location, i) {
        var address = location.address1;

        address += ", " + location.zipcode;

        var infoaddresswindow = location.brachlocation;
        if (infoaddresswindow !== "NULL") {
            infoaddresswindow += "<br/>";
        }
        infoaddresswindow += location.address1;
        if (location.address2.length > 0 || location.address2 !== null) {
            if (location.address2 !== "NULL") {
                infoaddresswindow += '<br/> ' + location.address2;
            }
        }
        infoaddresswindow += "<br/> " + location.city;
        infoaddresswindow += ", " + location.statename;
        infoaddresswindow += " " + location.zipcode;
        geocoder.geocode({ 'address': address }, function (results, status) {
            if (status == google.maps.GeocoderStatus.OK) {
                // got l & t now go for marker 

                var marker = new google.maps.Marker({
                    position: results[0].geometry.location,
                    map: map,
                    title: location.companydisplayname
                });

                arrMarkers[i] = marker;
                var infowindow = new google.maps.InfoWindow({
                    // content: "<h3>" + item.title + "</h3><p>" + item.description + "</p>"
                    content: "<h3>" + location.displayname + "</h3>" + infoaddresswindow + ""
                });
                arrInfoWindows[i] = infowindow;
                google.maps.event.addListener(marker, 'click', function () {
                    for (x = 0; x < arrInfoWindows.length; x++) { arrInfoWindows[x].close(); }
                    infowindow.open(map, marker);
                });

                //arrInfoWindows[i].open(map, arrMarkers[i]);
                //arrInfoWindows[i].close();

            } else {

                var marker = new google.maps.Marker({
                    position: new google.maps.LatLng(location.lat, location.lng),
                    map: map,
                    title: location.companydisplayname
                });

                arrMarkers[i] = marker;
                var infowindow = new google.maps.InfoWindow({
                    // content: "<h3>" + item.title + "</h3><p>" + item.description + "</p>"
                    content: location.displayname
                });
                arrInfoWindows[i] = infowindow;
                google.maps.event.addListener(marker, 'click', function () {
                    for (x = 0; x < arrInfoWindows.length; x++) { arrInfoWindows[x].close(); }
                    infowindow.open(map, marker);
                });
                //arrInfoWindows[i].open(map, arrMarkers[i]);
                //arrInfoWindows[i].close();
            }
        });
    }





    function addTableListItem() {
        //  alert("In AddTableListItem" + locations.length);
        var localTable = document.getElementById("dList");
        var innerHTML = "<div id='tableListView'>";

        for (var i = 0; i < locations.length; i++) {
            //  alert('MOD Value for ' + i + ' with 3 =  ' + ((i % 3))); 
            if (((i % 5) == 0) && (i > 0)) {
                innerHTML += "</div> ";
            }
            innerHTML += "<div id='listviewdata_" + i + "' class='listviewdata' >" + $("#listviewNOMAP_" + i).html() + "</div>";
        }
        var remaningTD = locations.length % 3;
        if (remaningTD > 0) {
            for (var i = 0; i < remaningTD; i++) {
                innerHTML += "<div id='listviewdata_' class='listviewdata'> </div>";
            }
        }

        innerHTML += "</div>";
        //alert(innerHTML); 
        localTable.innerHTML = innerHTML;

    }


    function addListViewItem(location, i) {
        // Clone duplicates the model (with bindings), strips model class/id, gives a unique id, and then adds it before the model
        $("#dModel").clone(true).removeAttr("id").removeClass("classModelItem").attr("id", "listview_" + i).appendTo("#dList")

        // The finds get each class from within the model and replace it with values.
	          .find(".CompanyName").html(location.companydisplayname).end()
               .find(".BranchLocation").html(location.brachlocation !== "NULL" ? location.brachlocation + '<br/>' : "").end()
	          .find(".Address_").html(Address(location.address1, location.address2)).end()
	          .find(".City").html(location.city).end()
	          .find(".StateName").html(location.statename + ' ' + location.zipcode).end()
	          .find(".Telephone1").html(location.telphone1).end()
	          .find(".distance").html(getstorehours(location.storehours1, location.storehours2));

    }

    function getstorehours(storehours1, storehours2) {
        var storehours = "";
        if (storehours1.length > 0 && storehours1 !== null) {
            if (storehours1 !== "null") {
                storehours = storehours1;
            }
        }
        else {
            if (storehours2 !== "null") {
                storehours = storehours2
            }
        }
        return storehours == "null" ? "" : storehours;

    }

    function addListViewItemNOMAP(location, i) {
        // Clone duplicates the model (with bindings), strips model class/id, gives a unique id, and then adds it before the model
        $("#dModelList").clone(true).removeAttr("id").removeClass("classModelItem").attr("id", "listviewNOMAP_" + i).appendTo("#dList")

        // The finds get each class from within the model and replace it with values.
	          .find(".CompanyName").html(location.companydisplayname).end()
              .find(".BranchLocation").html(location.brachlocation !== "NULL" ? location.brachlocation + '<br/>' : "").end()
	          .find(".Address_").html(Address(location.address1, location.address2)).end()
	          .find(".City").html(location.city).end()
	          .find(".StateName").html(location.statename + ' ' + location.zipcode).end()
	          .find(".Telephone1").html(location.telphone1).end()
	         .find(".distance").html(getstorehours(location.storehours1, location.storehours2));


    }




    function Address(address1, address2) {

        var address = address1
        if (address2.length > 0 || address2 !== null) {
            if (address2 !== "NULL") {
                address += '<br/>' + address2;
            }
        }
        return address;
    }
    function ShowView(ShowDivId, HideDivId) {
        // alert(ShowDivId);
        //alert('sss');
        if (ShowDivId == 'divListView') {
            //   alert('l');
            document.getElementById('TDdivMapView').className = 'btnThm1';
            document.getElementById('TDdivListView').className = 'darkbtn';

            $("#BtnSearch").click();

        }
        if (ShowDivId == 'divMapView') {
            //  alert('m');
            document.getElementById('TDdivListView').className = 'btnThm1';
            document.getElementById('TDdivMapView').className = 'darkbtn';
        }

        //callAjax();

        document.getElementById(ShowDivId).style.display = 'block';
        document.getElementById(HideDivId).style.display = 'none';
        return false;
    }


    function MShowView(MShowDivId, MHideDivId) {
       // alert('t');
        if (MShowDivId == 'MdivListView') {
            $('#divListView').show();
            $('#divMapView').hide();
            $('#MTDdivMapView').removeClass('darkbtn');
            $('#MTDdivMapView').addClass('btnThm1');
            $('#MTDdivListView').removeClass('btnThm1');
            $('#MTDdivListView').addClass('darkbtn');

        } else {
            $('#divListView').hide();
            $('#divMapView').show();
            $('#MTDdivListView').removeClass('darkbtn');
            $('#MTDdivListView').addClass('btnThm1');
            $('#MTDdivMapView').removeClass('btnThm1');
            $('#MTDdivMapView').addClass('darkbtn');


        }

    }
    function openPrintRetailerPopup(searchString) {

        var url = "https://www.IDoCollection.com/PrintLocation/";
        var clienturl = $("#ContentPlaceHolder1_hdnStoreName").val();
        if (clienturl.length >= 0)
            url = url + clienturl;

        var name = "PrintShoppingBagPopup";
        var w = 660;
        var h = 650;
        wleft = (screen.width - w) / 2;
        wtop = (screen.height - h) / 2;
        var win = window.open(url,
            name,
            'width=' + w + ', height=' + h + ', ' +
            'left=' + wleft + ', top=' + wtop + ', ' +
            'location=no, menubar=no, ' +
            'status=no, toolbar=no, scrollbars=no, resizable=no');

        // Just in case left and top are ignored
        win.moveTo(wleft, wtop);
        win.focus();
    }


