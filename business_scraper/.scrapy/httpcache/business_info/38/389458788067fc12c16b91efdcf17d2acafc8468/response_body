            this.webRootforimages = function webRootforimages(url) {
                if (url.search(/localhost/i) > 0)
                    url = "http://avalonwebserver.com";
                if (url.search(/192./i) > 0)
                    url = "http://avalonwebserver.com";
                return url;
            }
